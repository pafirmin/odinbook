/the_odin_project/nodeJs/social-network/client/src/components/posts/NewPost.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/PostList.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/NewsFeed.js",["231"],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/layout/UserPanel.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/socket/Socket.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/contexts/LoadingContext.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/search/SearchResults.js",["232"],"import React, { useEffect, useState } from \"react\";\nimport axios from \"axios\";\nimport { Link, useLocation } from \"react-router-dom\";\nimport UserCard from \"../users/UserCard\";\n\nconst useQuery = () => {\n  return new URLSearchParams(useLocation().search);\n};\n\nconst SearchResults = () => {\n  const query = useQuery();\n  const [results, setResults] = useState([]);\n\n  useEffect(() => {\n    document.title = `Search results - ${query.get(\"user\")}`;\n  }, [query]);\n\n  useEffect(() => {\n    const fetchResults = async () => {\n      try {\n        const res = await axios.get(`/api/search?user=${query.get(\"user\")}`);\n\n        setResults(res.data);\n      } catch (err) {\n        console.error(err);\n      }\n    };\n    fetchResults();\n  }, [query]);\n\n  return results.map((user) => <UserCard user={user} />);\n};\n\nexport default SearchResults;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/search/UserSearch.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/notifications/Notifications.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/notifications/NotificationListItem.js",["233","234"],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/notifications/FriendRequests.js",["235"],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/notifications/FriendRequestListItem.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/utils/Utils.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/utils/Alerts.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/utils/LoadingSpinner.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/utils/ScrollToTop.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/PostPage.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/hooks/useFriendshipStatus.js",["236"],"import { useEffect, useState } from \"react\";\n\nconst useFriendShipStatus = (currentUserID, user) => {\n  const [friendshipStatus, setFriendshipStatus] = useState(null);\n\n  useEffect(() => {\n    if (isCurrentUserProfile) {\n      setFriendshipStatus(\"isUser\");\n    } else if (userIsFriend) {\n      setFriendshipStatus(\"isFriend\");\n    } else if (requestIsPending) {\n      setFriendshipStatus(\"isPending\");\n    } else {\n      setFriendshipStatus(null);\n    }\n  }, [user]);\n\n  const isCurrentUserProfile = currentUserID === user._id;\n\n  const userIsFriend = user.friends.some(\n    (friend) =>\n      friend.user._id === currentUserID && friend.status === \"accepted\"\n  );\n\n  const requestIsPending = user.friends.some(\n    (friend) =>\n      friend.user._id === currentUserID && friend.status === \"recieved\"\n  );\n\n  return { friendshipStatus, setFriendshipStatus };\n};\n\nexport default useFriendShipStatus;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/UserCard.js",["237"],"import React from \"react\";\nimport { Link } from \"react-router-dom\";\n\nconst UserCard = ({ user }) => {\n  return (\n    <Link to={`/user/${user._id}`}>\n      <div\n        className=\"card\"\n        style={{ display: \"flex\", gap: \".5rem\", margin: \"8px 0\" }}\n      >\n        <img className=\"round thumbnail\" src={user.profilePic} />\n        <div\n          style={{\n            display: \"flex\",\n            flexDirection: \"column\",\n            justifyContent: \"center\",\n          }}\n        >\n          <h2 className=\"bold\">{user.fullName}</h2>\n          <span>{user.profile.location}</span>\n        </div>\n      </div>\n    </Link>\n  );\n};\n\nexport default UserCard;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/Friends.js",["238"],"import React, { useEffect, useState } from \"react\";\nimport axios from \"axios\";\nimport { useParams } from \"react-router-dom\";\nimport UserCard from \"./UserCard\";\n\nconst Friends = () => {\n  const { userID } = useParams();\n  const [user, setUser] = useState(null);\n\n  useEffect(() => {\n    fetchUser();\n  }, []);\n\n  useEffect(() => {\n    if (user) {\n      document.title = `Odinbook - ${user.firstName}'s friends`;\n    }\n  }, [user]);\n\n  const fetchUser = async () => {\n    try {\n      const res = await axios.get(`/api/users/${userID}`);\n\n      setUser(res.data);\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  return (\n    <div>\n      {user && <h2>{user.fullName}'s friends</h2>}\n      {user &&\n        user.friends.map((friend) => (\n          <UserCard key={friend._id} user={friend.user} />\n        ))}\n    </div>\n  );\n};\n\nexport default Friends;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/profile/EditProfile.js",["239","240"],"import React, { useContext, useState, useEffect } from \"react\";\nimport axios from \"axios\";\nimport { Button, TextInput } from \"../../utils/Utils\";\nimport { LoadingContext } from \"../../../contexts/LoadingContext\";\nimport { useHistory } from \"react-router-dom\";\nimport { AlertContext } from \"../../../contexts/AlertContext\";\n\nconst EditProfile = () => {\n  const history = useHistory();\n  const { setAlerts } = useContext(AlertContext);\n  const { setLoading } = useContext(LoadingContext);\n  const [image, setImage] = useState(\"\");\n  const [profileData, setProfileData] = useState({\n    location: \"\",\n    occupation: \"\",\n    bio: \"\",\n  });\n\n  useEffect(() => {\n    document.title = \"Edit Profile\";\n  }, []);\n\n  useEffect(() => {\n    const fetchProfile = async () => {\n      setLoading(true);\n      try {\n        const res = await axios.get(`/api/users/profile`);\n        const profile = res.data;\n\n        setProfileData(profile);\n      } catch (err) {\n        console.error(err);\n      }\n      setLoading(false);\n    };\n    fetchProfile();\n  }, []);\n\n  const handleFile = (e) => {\n    const file = e.target.files[0];\n    const reader = new FileReader();\n    reader.readAsDataURL(file);\n    reader.onloadend = () => {\n      setImage(reader.result);\n    };\n  };\n\n  const handleChange = (e) => {\n    setProfileData({\n      ...profileData,\n      [e.target.name]: e.target.value,\n    });\n  };\n\n  const handleSubmit = async (e) => {\n    e.preventDefault();\n    setLoading(true);\n    try {\n      const { location, occupation, bio } = profileData;\n      const body = JSON.stringify({ location, occupation, bio, image });\n\n      const res = await axios.put(`/api/users/profile`, body);\n      const userID = res.data;\n\n      history.push(`/user/${userID}`);\n    } catch (err) {\n      const errorArray = err.response.data.errors.map((err) => {\n        return { text: err.msg, type: \"danger\" };\n      });\n      setAlerts(errorArray);\n    } finally {\n      setLoading(false);\n      setTimeout(() => setAlerts([]), 5000);\n    }\n  };\n\n  return (\n    <div>\n      <form className=\"user-form\" onSubmit={(e) => handleSubmit(e)}>\n        <header className=\"form-header\">\n          <h2>Edit your profile</h2>\n        </header>\n        <div className=\"form-group\">\n          <label htmlFor=\"location\">Location:</label>\n          <TextInput\n            id=\"location\"\n            name=\"location\"\n            value={profileData.location}\n            onChange={(e) => handleChange(e)}\n          />\n        </div>\n        <div className=\"form-group\">\n          <label htmlFor=\"occupation\">Occupation:</label>\n          <TextInput\n            id=\"occupation\"\n            name=\"occupation\"\n            value={profileData.occupation}\n            onChange={(e) => handleChange(e)}\n          />\n        </div>\n        <div className=\"form-group\">\n          <label htmlFor=\"bio\">A little about yourself:</label>\n          <TextInput\n            id=\"bio\"\n            name=\"bio\"\n            value={profileData.bio}\n            onChange={(e) => handleChange(e)}\n          />\n        </div>\n        <div className=\"form-group\">\n          <label htmlFor=\"file\">Profile pic:</label>\n          <input\n            type=\"file\"\n            id=\"file\"\n            name=\"file\"\n            onChange={(e) => handleFile(e)}\n          />\n        </div>\n        {image && <img src={image} className=\"large thumbnail\" />}\n        <Button>Save changes</Button>\n      </form>\n    </div>\n  );\n};\n\nexport default EditProfile;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/profile/UserProfile.js",["241"],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/profile/FriendCard.js",["242"],"import React from \"react\";\nimport { Link } from \"react-router-dom\";\nconst FriendCard = ({ friend }) => {\n  return (\n    <div style={{ width: \"30%\", textAlign: \"center\" }}>\n      <Link to={`/user/${friend.user._id}`}>\n        <img className=\"large thumbnail\" src={friend.user.profilePic} />\n        <figcaption style={{ fontSize: \".85rem\", fontWeight: \"900\" }}>\n          {friend.user.fullName}\n        </figcaption>\n      </Link>\n    </div>\n  );\n};\n\nexport default FriendCard;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/profile/AddFriendBtn.js",["243"],"import axios from \"axios\";\nimport React, { useContext } from \"react\";\nimport styled from \"styled-components\";\nimport useFriendshipStatus from \"../../../hooks/useFriendshipStatus\";\nimport { AuthContext } from \"../../../contexts/AuthContext\";\nimport { Button } from \"../../utils/Utils\";\nimport { sendFriendRequest } from \"../../../socket/Socket\";\nimport { Link } from \"react-router-dom\";\n\nconst ProfileBtn = styled(Button)`\n  font-size: 0.8em;\n  padding: 2px 8px;\n`;\n\nconst AddFriendBtn = ({ user }) => {\n  const { authState } = useContext(AuthContext);\n  const { friendshipStatus, setFriendshipStatus } = useFriendshipStatus(\n    authState.userID,\n    user\n  );\n\n  const addFriend = async () => {\n    try {\n      const res = await axios.post(`/api/requests/${user._id}`, {});\n\n      sendFriendRequest(res.data);\n      setFriendshipStatus(\"isPending\");\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  const getButton = () => {\n    switch (friendshipStatus) {\n      case \"isUser\":\n        return null;\n      case \"isFriend\":\n        return (\n          <ProfileBtn variant=\"success\">\n            <i className=\"fas fa-check\" /> Friends\n          </ProfileBtn>\n        );\n      case \"isPending\":\n        return <ProfileBtn>Request sent!</ProfileBtn>;\n      default:\n        return (\n          <ProfileBtn onClick={addFriend}>\n            {\" \"}\n            <i className=\"fas fa-user-friends\" /> Add as friend\n          </ProfileBtn>\n        );\n    }\n  };\n  return getButton();\n};\n\nexport default AddFriendBtn;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/post/Post.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/post/PostHeader.js",["244"],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/post/comments/CommentSection.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/post/comments/Comment.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/post/comments/CommentForm.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/post/PostInteraction.js",["245"],"import React, { Fragment, useContext, useRef, useState } from \"react\";\nimport useIsLiked from \"../../../hooks/useIsLiked\";\nimport axios from \"axios\";\nimport styled from \"styled-components\";\nimport { AuthContext } from \"../../../contexts/AuthContext\";\nimport { sendNotification } from \"../../../socket/Socket\";\n\nconst SocialDiv = styled.div`\n  display: flex;\n  justify-content: space-between;\n  border-bottom: 1px solid #c6c6c6;\n  padding: 0.2rem 0;\n`;\n\nconst SocialIcon = styled.i`\n  font-size: 1.3em;\n  color: ${(props) => props.theme.mainColour};\n  padding-right: 0.5rem;\n`;\n\nconst LikeThumb = styled(SocialIcon)`\n  color: ${(props) => (props.isLiked ? \"#2d9ee9\" : props.theme.mainColour)};\n`;\n\nconst SocialBtn = styled.button`\n  border: none;\n  background: none;\n  font: inherit;\n  color: inherit;\n  cursor: pointer;\n  width: 50%;\n  border-radius: 20px;\n  padding: 0.2rem 0;\n\n  &:hover {\n    background-color: ${(props) => props.theme.secondaryBg};\n  }\n  &:hover > .fa-thumbs-up {\n    color: #2d9ee9;\n  }\n`;\n\nconst PostInteraction = ({ post }) => {\n  const { authState } = useContext(AuthContext);\n  const { comments } = post;\n  const [likes, setLikes] = useState(post.likes);\n  const isLiked = useIsLiked(likes, authState.userID);\n\n  const handleLike = async () => {\n    try {\n      const res = await axios.post(`/api/posts/${post._id}/like`, {});\n\n      if (!isLiked)\n        sendNotification({\n          sender: authState.userID,\n          recipient: post.user._id,\n          post: post._id,\n          type: \"like\",\n        });\n\n      setLikes(res.data);\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  const focusCommentBox = () => {\n    document.getElementById(`comment-box-${post._id}`).focus();\n  };\n\n  return (\n    <Fragment>\n      <SocialDiv>\n        <span>\n          {likes.length} like{likes.length !== 1 && \"s\"}\n        </span>\n        <span>\n          {comments.length} comment{comments.length !== 1 && \"s\"}\n        </span>\n      </SocialDiv>\n      <SocialDiv>\n        <SocialBtn onClick={handleLike}>\n          <LikeThumb isLiked={isLiked} className=\"fas fa-thumbs-up\" />\n          <span>{isLiked ? \"You liked this\" : \"Like\"}</span>\n        </SocialBtn>\n        <SocialBtn onClick={focusCommentBox}>\n          <SocialIcon className=\"far fa-comments\" />\n          Comment\n        </SocialBtn>\n      </SocialDiv>\n    </Fragment>\n  );\n};\n\nexport default PostInteraction;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/notifications/Messages.js",["246"],"import React, { useContext, useEffect, useState } from \"react\";\nimport axios from \"axios\";\nimport { AuthContext } from \"../../contexts/AuthContext\";\nimport { DropDown, Notification } from \"../utils/Utils\";\nimport MessagesListItem from \"./MessagesListItem\";\n\nconst Messages = ({ activeDropdown, toggleDropdown }) => {\n  const { authState } = useContext(AuthContext);\n  const [conversations, setConversations] = useState([]);\n  const [icon, setIcon] = useState(\"fas fa-envelope\");\n  const [unreadMessage, setUnreadMessage] = useState(false);\n\n  const handleNewMessage = () => {\n    setUnreadMessage(true);\n  };\n\n  useEffect(() => {\n    setIcon(activeDropdown === 1 ? \"fas fa-envelope-open\" : \"fas fa-envelope\");\n  }, [activeDropdown]);\n\n  useEffect(() => {\n    activeDropdown === 1 && fetchConversations();\n  }, [activeDropdown]);\n\n  useEffect(() => {\n    setUnreadMessage(conversations.some((convo) => !convo.lastMessage.seen));\n  }, [conversations]);\n\n  useEffect(() => {\n    const { socket } = authState;\n    socket.on(\"recieveMessage\", handleNewMessage);\n\n    return () => socket.off(\"recieveMessage\", handleNewMessage);\n  }, []);\n\n  const fetchConversations = async () => {\n    try {\n      const res = await axios.get(`/api/messaging`);\n\n      setConversations(res.data);\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  const getParticipant = (participants) => {\n    return participants.find((user) => user._id !== authState.userID);\n  };\n\n  return (\n    <div\n      style={{ position: \"relative\", display: \"flex\", alignItems: \"center\" }}\n    >\n      <div>\n        <i\n          style={{ fontSize: \"1.2em\", cursor: \"pointer\" }}\n          className={icon}\n          onClick={toggleDropdown}\n        />\n        {unreadMessage && <Notification />}\n        {activeDropdown === 1 && (\n          <DropDown style={{ overflow: \"visible\" }}>\n            <ul>\n              {conversations.map((convo) => (\n                <MessagesListItem\n                  key={convo._id}\n                  convo={convo}\n                  participant={getParticipant(convo.participants)}\n                />\n              ))}\n            </ul>\n          </DropDown>\n        )}\n      </div>\n    </div>\n  );\n};\n\nexport default Messages;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/chat/Conversation.js",["247","248","249"],"import React, { useContext, useEffect, useState, useRef } from \"react\";\nimport axios from \"axios\";\nimport Draggable from \"react-draggable\";\nimport { AlertContext } from \"../../contexts/AlertContext\";\nimport { AuthContext } from \"../../contexts/AuthContext\";\nimport { Button, TextInput } from \"../utils/Utils\";\nimport { sendMessage } from \"../../socket/Socket\";\nimport styled from \"styled-components\";\nimport ChatMessage from \"./ChatMessage\";\n\nconst ConvoWrapper = styled.div`\n  position: fixed;\n  width: 350px;\n  left: 70px;\n  top: 100px;\n  z-index: 5;\n  text-align: left;\n  background: #fff;\n  box-shadow: 0px 1px 2px #9d9d9d;\n`;\n\nconst ConvoHeader = styled.header`\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  cursor: pointer;\n  background: ${(props) => props.theme.headerColour};\n  color: #fff;\n  padding: 0.6rem;\n  font-size: 1.2rem;\n`;\n\nconst ConvoBody = styled.div`\n  overflow-y: scroll;\n  height: 400px;\n  display: flex;\n  flex-direction: column;\n  padding: 0.4rem;\n  background: ${(props) => props.theme.cardBg};\n  scrollbar-color: #bbb ${(props) => props.theme.cardBg};\n  scrollbar-width: thin;\n`;\n\nconst Conversation = ({\n  conversation,\n  participant,\n  setLastMessage,\n  setActive,\n}) => {\n  const scrollBottom = useRef(null);\n  const { authState } = useContext(AuthContext);\n  const { setAlerts } = useContext(AlertContext);\n  const [messages, setMessages] = useState([]);\n  const [newMessage, setNewMessage] = useState({ text: \"\" });\n  const [partnerIsTyping, setPartnerIsTyping] = useState(false);\n  const { socket } = authState;\n\n  useEffect(() => {\n    fetchMessages();\n  }, []);\n\n  useEffect(() => {\n    socket.on(\"recieveMessage\", handleRecieveMessage);\n\n    return () => socket.off(\"recieveMessage\", handleRecieveMessage);\n  }, []);\n\n  useEffect(() => {\n    socket.on(\"typing\", handleTyping);\n\n    return () => socket.off(\"typing\", handleTyping);\n  }, []);\n\n  useEffect(() => {\n    scrollBottom.current.scrollIntoView({ behavior: \"smooth\" });\n  }, [messages]);\n\n  const handleRecieveMessage = (newMessage) => {\n    setMessages((prev) => [...prev, newMessage]);\n\n    setLastMessage(newMessage);\n    setPartnerIsTyping(false);\n  };\n\n  const handleTyping = () => {\n    if (!partnerIsTyping) {\n      setPartnerIsTyping(true);\n\n      setTimeout(() => setPartnerIsTyping(false), 4000);\n    }\n  };\n\n  const fetchMessages = async () => {\n    try {\n      const res = await axios.get(`/api/messaging/chats/${conversation._id}`);\n\n      setMessages(res.data);\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  const handleChange = (e) => {\n    setNewMessage({ text: e.target.value });\n\n    !partnerIsTyping && socket.emit(\"typing\", participant._id);\n  };\n\n  const handleSubmit = async (e) => {\n    e.preventDefault();\n    try {\n      const body = JSON.stringify(newMessage);\n\n      const res = await axios.post(\n        `/api/messaging/send/${participant._id}`,\n        body\n      );\n\n      setMessages([...messages, res.data]);\n      setLastMessage(res.data);\n      setNewMessage({ text: \"\" });\n\n      sendMessage(res.data, participant._id);\n    } catch (err) {\n      const errorArray = err.response.data.errors.map((err) => {\n        return { text: err.msg, type: \"warning\" };\n      });\n      setAlerts(errorArray);\n    } finally {\n      setTimeout(() => setAlerts([]), 5000);\n    }\n  };\n\n  return (\n    <Draggable handle=\".handle\">\n      <ConvoWrapper>\n        <ConvoHeader className=\"bold handle\">\n          {participant.fullName}\n          <i className=\"far fa-times-circle\" onClick={() => setActive(false)} />\n        </ConvoHeader>\n        <ConvoBody>\n          {messages.map((msg) => (\n            <ChatMessage key={msg._id} message={msg} />\n          ))}\n          <div ref={scrollBottom} />\n        </ConvoBody>\n        {partnerIsTyping && <span>{participant.firstName} is typing...</span>}\n        <form onSubmit={(e) => handleSubmit(e)} style={{ display: \"flex\" }}>\n          <TextInput\n            autoComplete=\"off\"\n            type=\"text\"\n            name=\"text\"\n            value={newMessage.text}\n            onChange={(e) => handleChange(e)}\n            style={{ flexGrow: \"1\" }}\n          />\n          <Button>Send</Button>\n        </form>\n      </ConvoWrapper>\n    </Draggable>\n  );\n};\n\nexport default Conversation;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/notifications/MessagesListItem.js",["250"],"import React, { Fragment, useState } from \"react\";\nimport Conversation from \"../chat/Conversation\";\nimport { truncate } from \"lodash\";\n\nconst MessagesListItem = ({ convo, participant }) => {\n  const [active, setActive] = useState(false);\n  const [lastMessage, setLastMessage] = useState(convo.lastMessage);\n\n  const toggleActive = () => {\n    setActive(!active);\n  };\n\n  return (\n    <Fragment>\n      <li\n        onClick={toggleActive}\n        style={{\n          position: \"relative\",\n          cursor: \"pointer\",\n          padding: \"6px 0\",\n          borderBottom: \"1px solid #c6c6c6\",\n          textAlign: \"left\",\n        }}\n      >\n        <div\n          style={{\n            display: \"flex\",\n            gap: \".5rem\",\n          }}\n        >\n          <img\n            className=\"small round thumbnail\"\n            src={lastMessage.sender.profilePic}\n          />\n          <div style={{ display: \"flex\", flexDirection: \"column\" }}>\n            <p className=\"bold\">{participant.fullName}</p>\n            {lastMessage.sender.fullName}:{\" \"}\n            {truncate(lastMessage.text, { length: 24, omission: \"...\" })}\n          </div>\n        </div>\n      </li>\n      {active && (\n        <Conversation\n          conversation={convo}\n          participant={participant}\n          setLastMessage={setLastMessage}\n          setActive={setActive}\n        />\n      )}\n    </Fragment>\n  );\n};\n\nexport default MessagesListItem;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/chat/ChatMessage.js",["251","252"],"import React, { useContext } from \"react\";\nimport styled from \"styled-components\";\nimport { AuthContext } from \"../../contexts/AuthContext\";\nimport { format, formatDistanceToNow, parseISO } from \"date-fns\";\n\nconst MessageWrapper = styled.div`\n  position: relative;\n  background-color: ${(props) => props.theme.secondaryBg};\n  color: ${(props) => props.theme.mainFontColour};\n  border-radius: 20px;\n  padding: 1rem;\n  margin: 8px 6px 8px 0;\n  width: 66%;\n  align-self: ${({ sender, user }) =>\n    sender === user ? \"flex-end\" : \"flex-start\"};\n`;\n\nconst TimeStamp = styled.time`\n  position: absolute;\n  top: 0px;\n  left: 50%;\n  transform: translate(-50%);\n  font-size: 0.7rem;\n  color: ${(props) => props.theme.secondaryFontColour};\n  width: 100%;\n  text-align: center;\n`;\n\nconst MessageContainer = styled.div`\n  background: ${(props) => props.theme.secondaryBg};\n`;\n\nconst ChatMessage = ({ message }) => {\n  const { authState } = useContext(AuthContext);\n  return (\n    <MessageWrapper sender={message.sender._id} user={authState.userID}>\n      <TimeStamp title={format(parseISO(message.date), \"PPPppp\")}>\n        {formatDistanceToNow(parseISO(message.date), {\n          includeSeconds: true,\n          addSuffix: true,\n        })}\n      </TimeStamp>\n      <div>\n        <img className=\"tiny round thumbnail\" src={message.sender.profilePic} />{\" \"}\n        {message.text}\n      </div>{\" \"}\n    </MessageWrapper>\n  );\n};\n\nexport default ChatMessage;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/profile/MessageForm.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/profile/ProfileTop.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/profile/ProfileDetails.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/profile/ProfileFriends.js",["253"],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/UserPage.js",["254","255"],"import React, { useContext, useEffect, useState } from \"react\";\nimport axios from \"axios\";\nimport { useParams } from \"react-router-dom\";\nimport UserProfile from \"./profile/UserProfile\";\nimport PostList from \"../posts/PostList\";\nimport NewPost from \"../posts/NewPost\";\nimport { LoadingContext } from \"../../contexts/LoadingContext\";\nimport styled from \"styled-components\";\n\nconst UserPageWrapper = styled.div`\n  display: grid;\n  grid-template-columns: 1fr 2fr;\n  gap: 24px;\n\n  @media screen and (max-width: 900px) {\n    grid-template-columns: 1fr;\n  }\n`;\n\nconst UserPage = () => {\n  const { userID } = useParams();\n  const { setLoading } = useContext(LoadingContext);\n\n  const [user, setUser] = useState(null);\n  useEffect(() => {\n    const fetchUser = async () => {\n      setLoading(true);\n      try {\n        const res = await axios.get(`/api/users/${userID}`);\n\n        document.title = `Odinbook - ${res.data.fullName}`;\n        setUser(res.data);\n      } catch (err) {\n        console.error(err);\n      }\n    };\n    fetchUser();\n  }, [userID]);\n\n  const [posts, setPosts] = useState([]);\n  useEffect(() => {\n    const fetchPosts = async () => {\n      try {\n        const res = await axios.get(`/api/posts/user/${userID}/wall`);\n\n        setPosts(res.data);\n      } catch (err) {\n        console.error(err);\n      }\n      setLoading(false);\n    };\n    fetchPosts();\n  }, [userID]);\n\n  return (\n    <UserPageWrapper>\n      {user && <UserProfile user={user} />}\n      <div>\n        <NewPost setPosts={setPosts} userID={userID} />\n        <PostList posts={posts} />\n      </div>\n    </UserPageWrapper>\n  );\n};\n\nexport default UserPage;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/hooks/useIsLiked.js",["256"],"import { useCallback, useEffect, useState } from \"react\";\n\nconst useIsLiked = (likes, 