edia (max-width: 700px) {\n    border-radius: 0;\n  }\n`;\n\nconst SocialDiv = styled.div`\n  display: flex;\n  justify-content: space-between;\n  border-bottom: 1px solid #c6c6c6;\n  padding: 0.2rem 0;\n`;\n\nconst SocialIcon = styled.i`\n  font-size: 1.3em;\n  color: #6b6b6b;\n  padding-right: 0.5rem;\n`;\n\nconst LikeThumb = styled(SocialIcon)`\n  color: ${(props) => (props.isLiked ? \"#2d9ee9\" : \"#6b6b6b\")};\n\n  &:hover {\n    color: #2d9ee9;\n  }\n`;\n\nconst SocialBtn = styled.button`\n  border: none;\n  background: none;\n  font: inherit;\n  cursor: pointer;\n  width: 50%;\n  border-radius: 20px;\n  padding: 0.2rem 0;\n\n  &: hover {\n    background-color: #f5f5f5;\n  }\n`;\n\nconst Post = ({ post }) => {\n  const { state } = useContext(AuthContext);\n  const [likes, setLikes] = useState(post.likes);\n  const [isLiked, setIsLiked] = useState(false);\n  const [comments, setComments] = useState(post.comments);\n\n  useEffect(() => {\n    setIsLiked(likes.map((like) => like.user._id).includes(state.userID));\n  }, []);\n\n  const handleLike = async () => {\n    try {\n      const config = {\n        headers: {\n          Authorization: `bearer ${state.token}`,\n        },\n      };\n      const res = await axios.post(`/api/posts/${post._id}/like`, {}, config);\n\n      setLikes(res.data);\n      setIsLiked(!isLiked);\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  const focusCommentBox = () => {\n    document.getElementById(`comment-box-${post._id}`).focus();\n  };\n\n  return (\n    <PostContainer>\n      <div>\n        <Link to={`/user/${post.user}`}>\n          <h3 style={{ fontSize: \"1.2em\" }}>{post.name}</h3>\n        </Link>\n        <time style={{ fontSize: \"0.8em\", color: \"#626262\" }}>\n          {moment(post.date).fromNow()}\n        </time>\n      </div>\n      <div style={{ margin: \".8rem 0\" }}>{post.text}</div>\n      <SocialDiv>\n        <span>\n          {likes.length} like{likes.length !== 1 && \"s\"}\n        </span>\n        <span>\n          {comments.length} comment{comments.length !== 1 && \"s\"}\n        </span>\n      </SocialDiv>\n      <SocialDiv>\n        <SocialBtn onClick={handleLike}>\n          <LikeThumb isLiked={isLiked} className=\"fas fa-thumbs-up\" />\n          {isLiked ? \"You liked this\" : \"Like\"}\n        </SocialBtn>\n        <SocialBtn onClick={focusCommentBox}>\n          <SocialIcon className=\"far fa-comments\" />\n          Comment\n        </SocialBtn>\n      </SocialDiv>\n      <Comments comments={comments} setComments={setComments} post={post} />\n    </PostContainer>\n  );\n};\n\nexport default Post;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/comments/CommentSection.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/comments/CommentForm.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/comments/Comment.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/NewsFeed.js",["111"],"import axios from \"axios\";\nimport styled from \"styled-components\";\nimport React, { useContext, useEffect, useState } from \"react\";\nimport { AuthContext } from \"../../contexts/AuthContext\";\nimport NewPost from \"./NewPost\";\nimport PostList from \"./PostList\";\nimport { Button } from \"../Utils\";\n\nconst NewPostBtn = styled(Button)`\n  border-radius: 20px;\n  width: 70%;\n  margin: auto;\n\n  &:hover {\n    background-color: #2883bf;\n  }\n`;\n\nconst NewsFeed = () => {\n  const [posts, setPosts] = useState([]);\n  const [formShow, setFormShow] = useState(false);\n  const { state } = useContext(AuthContext);\n\n  useEffect(() => {\n    const fetchPosts = async () => {\n      try {\n        const config = {\n          headers: {\n            Authorization: `bearer ${state.token}`,\n          },\n        };\n        const res = await axios.get(\"/api/posts/feed\", config);\n\n        setPosts(res.data);\n      } catch (err) {\n        console.error(err);\n      }\n    };\n    fetchPosts();\n  }, []);\n\n  const toggleForm = () => {\n    setFormShow(!formShow);\n  };\n\n  return (\n    <div>\n      <div style={{ textAlign: \"center\" }}>\n        <NewPostBtn onClick={toggleForm}>Post something...</NewPostBtn>\n      </div>\n      <NewPost posts={posts} setPosts={setPosts} formShow={formShow} />\n      <PostList posts={posts} />\n    </div>\n  );\n};\n\nexport default NewsFeed;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/layout/UserPanel.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/FriendRequests.js",["112"],"import React, { useContext, useEffect, useState } from \"react\";\nimport axios from \"axios\";\nimport { AuthContext } from \"../contexts/AuthContext\";\nimport styled from \"styled-components\";\nimport FriendRequestListItem from \"./FriendRequestListItem\";\n\nconst Notification = styled.div`\n  border-radius: 50%;\n  background-color: #f00;\n  color: #fff;\n  font-size: 0.8rem;\n  width: 1.6em;\n  height: 1.6em;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  font-weight: 900;\n  margin-left: -3px;\n  box-shadow: 2px 1px 3px #616161;\n  position: absolute;\n  top: -0.1em;\n  right: -1.2em;\n`;\n\nconst DropDown = styled.div`\n  display: ${({ show }) => (show ? \"block\" : \"none\")};\n  color: #434343;\n  position: absolute;\n  width: 300px;\n  height: 200px;\n  background-color: #fff;\n  box-shadow: 2px 2px 8px #7d7d7d;\n  padding: 0.5rem;\n  text-align: center;\n`;\n\nconst FriendRequest = () => {\n  const [requests, setRequests] = useState([]);\n  const [showDropDown, setShowDropdown] = useState(false);\n  const { state } = useContext(AuthContext);\n\n  useEffect(() => {\n    fetchRequests();\n  }, []);\n\n  const fetchRequests = async () => {\n    try {\n      const config = {\n        headers: {\n          Authorization: `bearer ${state.token}`,\n        },\n      };\n\n      const res = await axios.get(`/api/requests`, config);\n\n      setRequests(res.data);\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  const toggleDropDown = () => {\n    !showDropDown && fetchRequests();\n    setShowDropdown(!showDropDown);\n  };\n\n  return (\n    <div\n      style={{ position: \"relative\", display: \"flex\", alignItems: \"center\" }}\n    >\n      <div>\n        <i\n          style={{ fontSize: \"1.2em\", cursor: \"pointer\" }}\n          className=\"fas fa-user-friends\"\n          onClick={toggleDropDown}\n        />\n        {requests.length > 0 && <Notification>{requests.length}</Notification>}\n        <DropDown show={showDropDown}>\n          <span>Friend requests</span>\n          <ul>\n            {requests.map((request) => (\n              <FriendRequestListItem\n                key={request._id}\n                user={request.user}\n                requests={requests}\n                setRequests={setRequests}\n                requestID={request._id}\n              />\n            ))}\n          </ul>\n          {!requests.length && (\n            <div\n              style={{\n                color: \"#9d9d9d\",\n                marginTop: \"3em\",\n              }}\n            >\n              You have no pending friend requests\n            </div>\n          )}\n        </DropDown>\n      </div>\n    </div>\n  );\n};\n\nexport default FriendRequest;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/FriendRequestListItem.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/SearchResults.js",["113"],{"ruleId":"114","replacedBy":"115"},{"ruleId":"116","replacedBy":"117"},{"ruleId":"118","severity":1,"message":"119","line":23,"column":6,"nodeType":"120","endLine":23,"endColumn":8,"suggestions":"121"},{"ruleId":"118","severity":1,"message":"119","line":36,"column":6,"nodeType":"120","endLine":36,"endColumn":8