tate([]);\n  const [skip, setSkip] = useState(0);\n\n  useEffect(() => {\n    document.title = \"Odinbook - Newsfeed\";\n  }, []);\n\n  useEffect(() => {\n    window.addEventListener(\"scroll\", debounce(handleScroll, 500));\n\n    return () => window.removeEventListener(\"scroll\", handleScroll);\n  }, []);\n\n  useEffect(() => {\n    const fetchPosts = async () => {\n      setLoading(true);\n      try {\n        const res = await axios.get(`/api/posts/feed?skip=${skip}`);\n\n        setPosts((prev) => [...prev, ...res.data]);\n      } catch (err) {\n        console.error(err);\n      } finally {\n        setLoading(false);\n      }\n    };\n    fetchPosts();\n  }, [skip]);\n\n  const handleScroll = () => {\n    if (window.innerHeight + window.scrollY >= document.body.scrollHeight) {\n      setSkip((prev) => prev + 10);\n    }\n  };\n\n  return (\n    <div style={{ maxWidth: \"800px\", margin: \"auto\" }}>\n      <NewPost setPosts={setPosts} />\n      <PostList posts={posts} />\n    </div>\n  );\n};\n\nexport default NewsFeed;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/layout/UserPanel.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/socket/Socket.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/contexts/LoadingContext.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/search/SearchResults.js",["251"],"import React, { useEffect, useState } from \"react\";\nimport axios from \"axios\";\nimport { Link, useLocation } from \"react-router-dom\";\nimport UserCard from \"../users/UserCard\";\n\nconst useQuery = () => {\n  return new URLSearchParams(useLocation().search);\n};\n\nconst SearchResults = () => {\n  const query = useQuery();\n  const [results, setResults] = useState([]);\n\n  useEffect(() => {\n    document.title = `Search results - ${query.get(\"user\")}`;\n  }, [query]);\n\n  useEffect(() => {\n    const fetchResults = 