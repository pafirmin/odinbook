js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/layout/MainContent.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/layout/Header.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/NewPost.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/PostList.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/NewsFeed.js",["237"],"import axios from \"axios\";\nimport React, { useContext, useEffect, useState } from \"react\";\nimport { LoadingContext } from \"../../contexts/LoadingContext\";\nimport NewPost from \"./NewPost\";\nimport PostList from \"./PostList\";\nimport { debounce } from \"lodash\";\n\nconst NewsFeed = () => {\n  const { setLoading } = useContext(LoadingContext);\n  const [posts, setPosts] = useState([]);\n  const [skip, setSkip] = useState(0);\n\n  useEffect(() => {\n    document.title = \"Odinbook - Newsfeed\";\n  }, []);\n\n  useEffect(() => {\n    window.addEventListener(\"scroll\", debounce(handleScroll, 500));\n\n    return () => window.removeEventListener(\"scroll\", handleScroll);\n  }, []);\n\n  useEffect(() => {\n    const fetchPosts = async () => {\n      setLoading(true);\n      try {\n        const res = await axios.get(`/api/posts/feed?skip=${skip}`);\n\n        setPosts((prev) => [...prev, ...res.data]);\n      } catch (err) {\n        console.error(err);\n      } finally {\n        setLoading(false);\n      }\n    };\n    fetchPosts();\n  }, [skip]);\n\n  const handleScroll = () => {\n    if (window.innerHeight + window.scrollY >= document.body.scrollHeight) {\n      setSkip((prev) => prev + 10);\n    }\n  };\n\n  return (\n    <div style={{ maxWidth: \"800px\", margin: \"auto\" }}>\n      <NewPost setPosts={setPosts} />\n      <PostList posts={posts} />\n    </div>\n  );\n};\n\nexport default NewsFeed;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/layout/UserPanel.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/socket/Socket.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/contexts/LoadingContext.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/search/SearchResults.js",["238"],"import React, { useEffect, useState } from \"react\";\nimport axios from \"axios\";\nimport { Link, useLocation } from \"react-router-dom\";\nimport UserCard from \"../users/UserCard\";\n\nconst useQuery = () => {\n  return new URLSearchParams(useLocation().search);\n};\n\nconst SearchResults = () => {\n  const query = useQuery();\n  const [results, setResults] = useState([]);\n\n  useEffect(() => {\n    document.title = `Search results - ${query.get(\"user\")}`;\n  }, [query]);\n\n  useEffect(() => {\n    const fetchResults = async () => {\n      try {\n        const res = await axios.get(`/api/search?user=${query.get(\"user\")}`);\n\n        setResults(res.data);\n      } catch (err) {\n        console.error(err);\n      }\n    };\n    fetchResults();\n  }, [query]);\n\n  return results.map((user) => <UserCard user={user} />);\n};\n\nexport default SearchResults;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/search/UserSearch.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/notifications/Notifications.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/notifications/NotificationListItem.js",["239","240"],"import React, { useEffect, useReducer, useState } from \"react\";\nimport { Link } from \"react-router-dom\";\nimport { formatDistanceToNow, parseISO } from \"date-fns\";\nimport styled from \"styled-components\";\n\nconst NotificationContainer = styled.div`\n  display: flex;\n  align-items: center;\n  border-bottom: 1px solid ${(props) => props.theme.borderColour};\n  border-radius: 6px;\n  padding: 0.4rem;\n  gap: 0.4rem;\n  margin: 0.2rem 0;\n  background-color: ${(props) => (props.seen ? props.theme.cardBg : \"#e6efff\")};\n`;\n\nconst notificationReducer = (state, action) => {\n  switch (action.type) {\n    case \"like\":\n      return { message: ` liked your post` };\n    case \"commentLike\":\n      return { message: ` liked your comment` };\n    case \"comment\":\n      return { message: ` commented on your post` };\n    case \"post\":\n      return { message: ` posted on your wall` };\n    default:\n      return state;\n  }\n};\n\nconst NotificationListItem = ({ notification }) => {\n  const { sender, post, type, date } = notification;\n  const initialState = { message: null };\n  const [state, dispatch] = useReducer(notificationReducer, initialState);\n  const [seen, setSeen] = useState(notification.seen);\n\n  useEffect(() => {\n    dispatch({ type });\n  }, []);\n\n  return (\n    <li onClick={() => setSeen(true)}>\n      <Link to={`/posts/${post}`}>\n        <NotificationContainer seen={seen}>\n          <img className=\"small round thumbnail\" src={sender.profilePic} />\n          <div\n            style={{\n              display: \"flex\",\n              flexDirection: \"column\",\n              textAlign: \"left\",\n            }}\n          >\n            <span>\n              <span className=\"bold\">{sender.fullName}</span> {state.message}\n            </span>\n            <time style={{ color: \"#7a7a7a\" }}>\n              {formatDistanceToNow(parseISO(date), {\n                includeSeconds: true,\n                addSuffix: true,\n              })}\n            </time>\n          </div>\n        </NotificationContainer>\n      </Link>\n    </li>\n  );\n};\n\nexport default NotificationListItem;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/notifications/FriendRequests.js",["241"],"import React, { useContext, useEffect, useState } from \"react\";\nimport axios from \"axios\";\nimport { AuthContext } from \"../../contexts/AuthContext\";\nimport FriendRequestListItem from \"./FriendRequestListItem\";\nimport { DropDown, Notification } from \"../utils/Utils\";\n\nconst FriendRequests = ({ activeDropdown, toggleDropdown }) => {\n  const [requests, setRequests] = useState([]);\n  const { authState } = useContext(AuthContext);\n\n  const handleRecieveRequest = (request) => setRequests([request, ...requests]);\n\n  useEffect(() => {\n    activeDropdown === 3 && fetchRequests();\n  }, [authState.user, activeDropdown]);\n\n  useEffect(() => {\n    const { socket } = authState;\n    socket.on(\"recieveRequest\", handleRecieveRequest);\n\n    return () => socket.off(\"recieveRequest\", handleRecieveRequest);\n  }, []);\n\n  const fetchRequests = async () => {\n    try {\n      const res = await axios.get(`/api/requests`);\n\n      setRequests(res.data);\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  return (\n    <div\n      style={{ position: \"relative\", display: \"flex\", alignItems: \"center\" }}\n    >\n      <div>\n        <i\n          style={{ fontSize: \"1.2em\", cursor: \"pointer\" }}\n          className=\"fas fa-user-friends\"\n          onClick={toggleDropdown}\n        />\n        {requests.length > 0 && <Notification>{requests.length}</Notification>}\n        {activeDropdown === 3 && (\n          <DropDown>\n            <span>Friend requests</span>\n            <ul>\n              {requests.map((request) => (\n                <FriendRequestListItem\n                  key={request._id}\n                  user={request.user}\n                  requests={requests}\n                  setRequests={setRequests}\n                  requestID={request._id}\n                />\n              ))}\n            </ul>\n            {!requests.length && (\n              <div\n                style={{\n                  color: \"#9d9d9d\",\n                  marginTop: \"3em\",\n                }}\n              >\n                You have no pending friend requests\n              </div>\n            )}\n          </DropDown>\n        )}\n      </div>\n    </div>\n  );\n};\n\nexport default FriendRequests;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/notifications/FriendRequestListItem.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/utils/Utils.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/utils/Alerts.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/utils/LoadingSpinner.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/utils/ScrollToTop.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/PostPage.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/hooks/useFriendshipStatus.js",["242"],"import { useEffect, useState } from \"react\";\n\nconst useFriendShipStatus = (currentUserID, user) => {\n  const [friendshipStatus, setFriendshipStatus] = useState(null);\n\n  useEffect(() => {\n    if (isCurrentUserProfile) {\n      setFriendshipStatus(\"isUser\");\n    } else if (userIsFriend) {\n      setFriendshipStatus(\"isFriend\");\n    } else if (requestIsPending) {\n      setFriendshipStatus(\"isPending\");\n    } else {\n      setFriendshipStatus(null);\n    }\n  }, [user]);\n\n  const isCurrentUserProfile = currentUserID === user._id;\n\n  const userIsFriend = user.friends.some(\n    (friend) =>\n      friend.user._id === currentUserID && friend.status === \"accepted\"\n  );\n\n  const requestIsPending = user.friends.some(\n    (friend) =>\n      friend.user._id === currentUserID && friend.status === \"recieved\"\n  );\n\n  return { friendshipStatus, setFriendshipStatus };\n};\n\nexport default useFriendShipStatus;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/UserCard.js",["243"],"import React from \"react\";\nimport { Link } from \"react-router-dom\";\n\nconst UserCard = ({ user }) => {\n  return (\n    <Link to={`/user/${user._id}`}>\n      <div\n        className=\"card\"\n        style={{ display: \"flex\", gap: \".5rem\", margin: \"8px 0\" }}\n      >\n        <img className=\"round thumbnail\" src={user.profilePic} />\n        <div\n          style={{\n            display: \"flex\",\n            flexDirection: \"column\",\n            justifyContent: \"center\",\n          }}\n        >\n          <h2 className=\"bold\">{user.fullName}</h2>\n          <span>{user.profile.location}</span>\n        </div>\n      </div>\n    </Link>\n  );\n};\n\nexport default UserCard;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/Friends.js",["244"],"import React, { useEffect, useState } from \"react\";\nimport axios from \"axios\";\nimport { useParams } from \"react-router-dom\";\nimport UserCard from \"./UserCard\";\n\nconst Friends = () => {\n  const { userID } = useParams();\n  const [user, setUser] = useState(null);\n\n  useEffect(() => {\n    fetchUser();\n  }, []);\n\n  useEffect(() => {\n    if (user) {\n      document.title = `Odinbook - ${user.firstName}'s friends`;\n    }\n  }, [user]);\n\n  const fetchUser = async () => {\n    try {\n      const res = await axios.get(`/api/users/${userID}`);\n\n      setUser(res.data);\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  return (\n    <div>\n      {user && <h2>{user.fullName}'s friends</h2>}\n      {user &&\n        user.friends.map((friend) => (\n          <UserCard key={friend._id} user={friend.user} />\n        ))}\n    </div>\n  );\n};\n\nexport default Friends;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/profile/EditProfile.js",["245","246"],"import React, { useContext, useState, useEffect } from \"react\";\nimport axios from \"axios\";\nimport { Button, TextInput } from \"../../utils/Utils\";\nimport { LoadingContext } from \"../../../contexts/LoadingContext\";\nimport { useHistory } from \"react-router-dom\";\nimport { AlertContext } from \"../../../contexts/AlertContext\";\n\nconst EditProfile = () => {\n  const history = useHistory();\n  const { setAlerts } = useContext(AlertContext);\n  const { setLoading } = useContext(LoadingContext);\n  const [image, setImage] = useState(\"\");\n  const [profileData, setProfileData] = useState({\n    location: \"\",\n    occupation: \"\",\n    bio: \"\",\n  });\n\n  useEffect(() => {\n    document.title = \"Edit Profile\";\n  }, []);\n\n  useEffect(() => {\n    const fetchProfile = async () => {\n      setLoading(true);\n      try {\n        const res = await axios.get(`/api/users/profile`);\n        const profile = res.data;\n\n        setProfileData(profile);\n      } catch (err) {\n        console.error(err);\n      }\n      setLoading(false);\n    };\n    fetchProfile();\n  }, []);\n\n  const handleFile = (e) => {\n    const file = e.target.files[0];\n    const reader = new FileReader();\n    reader.readAsDataURL(file);\n    reader.onloadend = () => {\n      setImage(reader.result);\n    };\n  };\n\n  const handleChange = (e) => {\n    setProfileData({\n      ...profileData,\n      [e.target.name]: e.target.value,\n    });\n  };\n\n  const handleSubmit = async (e) => {\n    e.preventDefault();\n    setLoading(true);\n    try {\n      const { location, occupation, bio } = profileData;\n      const body = JSON.stringify({ location, occupation, bio, image });\n\n      const res = await axios.put(`/api/users/profile`, body);\n      const userID = res.data;\n\n      history.push(`/user/${userID}`);\n    } catch (err) {\n      const errorArray = err.response.data.errors.map((err) => {\n        return { text: err.msg, type: \"danger\" };\n      });\n      setAlerts(errorArray);\n    } finally {\n      setLoading(false);\n      setTimeout(() => setAlerts([]), 5000);\n    }\n  };\n\n  return (\n    <div>\n      <form className=\"user-form\" onSubmit={(e) => handleSubmit(e)}>\n        <header className=\"form-header\">\n          <h2>Edit your profile</h2>\n        </header>\n        <div className=\"form-group\">\n          <label htmlFor=\"location\">Location:</label>\n          <TextInput\n            id=\"location\"\n            name=\"location\"\n            value={profileData.location}\n            onChange={(e) => handleChange(e)}\n          />\n        </div>\n        <div className=\"form-group\">\n          <label htmlFor=\"occupation\">Occupation:</label>\n          <TextInput\n            id=\"occupation\"\n            name=\"occupation\"\n            value={profileData.occupation}\n            onChange={(e) => handleChange(e)}\n          />\n        </div>\n        <div className=\"form-group\">\n          <label htmlFor=\"bio\">A little about yourself:</label>\n          <TextInput\n            id=\"bio\"\n            name=\"bio\"\n            value={profileData.bio}\n            onChange={(e) => handleChange(e)}\n          />\n        </div>\n        <div className=\"form-group\">\n          <label htmlFor=\"file\">Profile pic:</label>\n          <input\n            type=\"file\"\n            id=\"file\"\n            name=\"file\"\n            onChange={(e) => handleFile(e)}\n          />\n        </div>\n        {image && <img src={image} className=\"large thumbnail\" />}\n        <Button>Save changes</Button>\n      </form>\n    </div>\n  );\n};\n\nexport default EditProfile;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/profile/UserProfile.js",["247"],"import React, { useEffect, useContext, useState, useCallback } from \"react\";\nimport { useMediaQuery } from \"react-responsive\";\nimport styled from \"styled-components\";\nimport { AuthContext } from \"../../../contexts/AuthContext\";\nimport useFriendshipStatus from \"../../../hooks/useFriendshipStatus\";\nimport ProfileTop from \"./ProfileTop\";\nimport ProfileDetails from \"./ProfileDetails\";\nimport ProfileFriends from \"./ProfileFriends\";\nimport { sampleSize } from \"lodash\";\n\nconst ProfileWrapper = styled.div`\n  margin-top: 16px;\n  display: flex;\n  flex-direction: column;\n  gap: 12px;\n  position: sticky;\n  top: ${({ top }) => top}px;\n`;\n\nconst ProfileSection = styled.section`\n  padding: 0.8rem;\n  background: ${(props) => props.theme.cardBg};\n  border-radius: 8px;\n  box-shadow: 0px 1px 2px ${(props) => props.theme.shadowColour};\n  display: flex;\n  flex-direction: column;\n  gap: 0.8rem;\n\n  & a:hover {\n    text-decoration: underline;\n  }\n`;\n\nconst Profile = ({ user }) => {\n  const { authState } = useContext(AuthContext);\n  const { friendshipStatus } = useFriendshipStatus(authState.userID, user);\n  const [friends, setFriends] = useState([]);\n  const [stickyTop, setStickyTop] = useState();\n  const isMobile = useMediaQuery({ query: \"(max-width: 800px)\" });\n\n  const filteredFriends = useCallback(() => {\n    return user.friends.filter((friend) => friend.status === \"accepted\");\n  }, [user]);\n\n  const stickyRef = useCallback(\n    (node) => {\n      if (node) {\n        setStickyTop(window.innerHeight - node.scrollHeight);\n      }\n    },\n    [friends]\n  );\n\n  useEffect(() => {\n    setFriends(sampleSize(filteredFriends(), isMobile ? 6 : 9));\n  }, [filteredFriends, isMobile]);\n\n  return (\n    <div>\n      <ProfileWrapper top={stickyTop} ref={stickyRef}>\n        <ProfileTop user={user} friendshipStatus={friendshipStatus} />\n        <ProfileSection>\n          <ProfileDetails user={user} friendshipStatus={friendshipStatus} />\n        </ProfileSection>\n        <ProfileSection>\n          <ProfileFriends user={user} friends={friends} />\n        </ProfileSection>\n        {!isMobile && (\n          <footer style={{ textAlign: \"center\" }}>\n            'Odinbook', created by Paul Firmin, 2021\n          </footer>\n        )}\n      </ProfileWrapper>\n    </div>\n  );\n};\n\nexport default Profile;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/profile/FriendCard.js",["248"],"import React from \"react\";\nimport { Link } from \"react-router-dom\";\nconst FriendCard = ({ friend }) => {\n  return (\n    <div style={{ width: \"30%\", textAlign: \"center\" }}>\n      <Link to={`/user/${friend.user._id}`}>\n        <img className=\"large thumbnail\" src={friend.user.profilePic} />\n        <figcaption style={{ fontSize: \".85rem\", fontWeight: \"900\" }}>\n          {friend.user.fullName}\n        </figcaption>\n      </Link>\n    </div>\n  );\n};\n\nexport default FriendCard;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/profile/AddFriendBtn.js",["249"],"import axios from \"axios\";\nimport React, { useContext } from \"react\";\nimport styled from \"styled-components\";\nimport useFriendshipStatus from \"../../../hooks/useFriendshipStatus\";\nimport { AuthContext } from \"../../../contexts/AuthContext\";\nimport { Button } from \"../../utils/Utils\";\nimport { sendFriendRequest } from \"../../../socket/Socket\";\nimport { Link } from \"react-router-dom\";\n\nconst ProfileBtn = styled(Button)`\n  font-size: 0.8em;\n  padding: 2px 8px;\n`;\n\nconst AddFriendBtn = ({ user }) => {\n  const { authState } = useContext(AuthContext);\n  const { friendshipStatus, setFriendshipStatus } = useFriendshipStatus(\n    authState.userID,\n    user\n  );\n\n  const addFriend = async () => {\n    try {\n      const res = await axios.post(`/api/requests/${user._id}`, {});\n\n      sendFriendRequest(res.data);\n      setFriendshipStatus(\"isPending\");\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  const getButton = () => {\n    switch (friendshipStatus) {\n      case \"isUser\":\n        return null;\n      case \"isFriend\":\n        return (\n          <ProfileBtn variant=\"success\">\n            <i className=\"fas fa-check\" /> Friends\n          </ProfileBtn>\n        );\n      case \"isPending\":\n        return <ProfileBtn>Request sent!</ProfileBtn>;\n      default:\n        return (\n          <ProfileBtn onClick={addFriend}>\n            {\" \"}\n            <i className=\"fas fa-user-friends\" /> Add as friend\n          </ProfileBtn>\n        );\n    }\n  };\n  return getButton();\n};\n\nexport default AddFriendBtn;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/post/Post.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/post/PostHeader.js",["250"],"import React from \"react\";\nimport { format, formatDistanceToNow, parseISO } from \"date-fns\";\nimport { Link } from \"react-router-dom\";\n\nconst PostHeader = ({ post }) => {\n  const { user, date, name, recipient, recipientName } = post;\n\n  return (\n    <div style={{ display: \"flex\", gap: \"8px\" }}>\n      <img className=\"small round thumbnail\" src={user.profilePic} />\n      <div\n        style={{\n          display: \"flex\",\n          flexDirection: \"column\",\n          justifyContent: \"space-between\",\n        }}\n      >\n        {recipient !== user._id ? (\n          <h3 style={{ fontSize: \"1.1em\" }}>\n            <Link className=\"bold\" to={`/user/${user._id}`}>\n              {name}\n            </Link>{\" \"}\n            posted on{\" \"}\n            <Link className=\"bold\" to={`/user/${recipient}`}>\n              {recipientName}\n            </Link>\n            's wall\n          </h3>\n        ) : (\n          <Link className=\"bold\" to={`/user/${user._id}`}>\n            <h3 style={{ fontSize: \"1.1em\" }}>{name}</h3>\n          </Link>\n        )}\n        <time\n          title={format(parseISO(date), \"PPPppp\")}\n          style={{ fontSize: \"0.8em\", color: \"#626262\" }}\n        >\n          {formatDistanceToNow(parseISO(date), {\n            includeSeconds: true,\n            addSuffix: true,\n          })}\n        </time>\n      </div>\n    </div>\n  );\n};\n\nexport default PostHeader;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/post/comments/CommentSection.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/post/comments/Comment.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/post/comments/CommentForm.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/post/PostInteraction.js",["251"],"import React, { Fragment, useContext, useRef, useState } from \"react\";\nimport useIsLiked from \"../../../hooks/useIsLiked\";\nimport axios from \"axios\";\nimport styled from \"styled-components\";\nimport { AuthContext } from \"../../../contexts/AuthContext\";\nimport { sendNotification } from \"../../../socket/Socket\";\n\nconst SocialDiv = styled.div`\n  display: flex;\n  justify-content: space-between;\n  border-bottom: 1px solid #c6c6c6;\n  padding: 0.2rem 0;\n`;\n\nconst SocialIcon = styled.i`\n  font-size: 1.3em;\n  color: ${(props) => props.theme.mainColour};\n  padding-right: 0.5rem;\n`;\n\nconst LikeThumb = styled(SocialIcon)`\n  color: ${(props) => (props.isLiked ? \"#2d9ee9\" : props.theme.mainColour)};\n`;\n\nconst SocialBtn = styled.button`\n  border: none;\n  background: none;\n  font: inherit;\n  color: inherit;\n  cursor: pointer;\n  width: 50%;\n  border-radius: 20px;\n  padding: 0.2rem 0;\n\n  &:hover {\n    background-color: ${(props) => props.theme.secondaryBg};\n  }\n  &:hover > .fa-thumbs-up {\n    color: #2d9ee9;\n  }\n`;\n\nconst PostInteraction = ({ post }) => {\n  const { authState } = useContext(AuthContext);\n  const { comments } = post;\n  const [likes, setLikes] = useState(post.likes);\n  const isLiked = useIsLiked(likes, authState.userID);\n\n  const handleLike = async () => {\n    try {\n      const res = await axios.post(`/api/posts/${post._id}/like`, {});\n\n      if (!isLiked)\n        sendNotification({\n          sender: authState.userID,\n          recipient: post.user._id,\n          post: post._id,\n          type: \"like\",\n        });\n\n      setLikes(res.data);\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  const focusCommentBox = () => {\n    document.getElementById(`comment-box-${post._id}`).focus();\n  };\n\n  return (\n    <Fragment>\n      <SocialDiv>\n        <span>\n          {likes.length} like{likes.length !== 1 && \"s\"}\n        </span>\n        <span>\n          {comments.length} comment{comments.length !== 1 && \"s\"}\n        </span>\n      </SocialDiv>\n      <SocialDiv>\n        <SocialBtn onClick={handleLike}>\n          <LikeThumb isLiked={isLiked} className=\"fas fa-thumbs-up\" />\n          <span>{isLiked ? \"You liked this\" : \"Like\"}</span>\n        </SocialBtn>\n        <SocialBtn onClick={focusCommentBox}>\n          <SocialIcon className=\"far fa-comments\" />\n          Comment\n        </SocialBtn>\n      </SocialDiv>\n    </Fragment>\n  );\n};\n\nexport default PostInteraction;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/notifications/Messages.js",["252"],"import React, { useContext, useEffect, useState } from \"react\";\nimport axios from \"axios\";\nimport { AuthContext } from \"../../contexts/AuthContext\";\nimport { DropDown, Notification } from \"../utils/Utils\";\nimport MessagesListItem from \"./MessagesListItem\";\n\nconst Messages = ({ activeDropdown, toggleDropdown }) => {\n  const { authState } = useContext(AuthContext);\n  const [conversations, setConversations] = useState([]);\n  const [icon, setIcon] = useState(\"fas fa-envelope\");\n  const [unreadMessage, setUnreadMessage] = useState(false);\n\n  const handleNewMessage = () => {\n    setUnreadMessage(true);\n  };\n\n  useEffect(() => {\n    setIcon(activeDropdown === 1 ? \"fas fa-envelope-open\" : \"fas fa-envelope\");\n  }, [activeDropdown]);\n\n  useEffect(() => {\n    activeDropdown === 1 && fetchConversations();\n  }, [activeDropdown]);\n\n  useEffect(() => {\n    setUnreadMessage(conversations.some((convo) => !convo.lastMessage.seen));\n  }, [conversations]);\n\n  useEffect(() => {\n    const { socket } = authState;\n    socket.on(\"recieveMessage\", handleNewMessage);\n\n    return () => socket.off(\"recieveMessage\", handleNewMessage);\n  }, []);\n\n  const fetchConversations = async () => {\n    try {\n      const res = await axios.get(`/api/messaging`);\n\n      setConversations(res.data);\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  const getParticipant = (participants) => {\n    return participants.find((user) => user._id !== authState.userID);\n  };\n\n  return (\n    <div\n      style={{ position: \"relative\", display: \"flex\", alignItems: \"center\" }}\n    >\n      <div>\n        <i\n          style={{ fontSize: \"1.2em\", cursor: \"pointer\" }}\n          className={icon}\n          onClick={toggleDropdown}\n        />\n        {unreadMessage && <Notification />}\n        {activeDropdown === 1 && (\n          <DropDown style={{ overflow: \"visible\" }}>\n            <ul>\n              {conversations.map((convo) => (\n                <MessagesListItem\n                  key={convo._id}\n                  convo={convo}\n                  participant={getParticipant(convo.participants)}\n                />\n              ))}\n            </ul>\n          </DropDown>\n        )}\n      </div>\n    </div>\n  );\n};\n\nexport default Messages;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/chat/Conversation.js",["253","254","255"],"import React, { useContext, useEffect, useState, useRef } from \"react\";\nimport axios from \"axios\";\nimport Draggable from \"react-draggable\";\nimport { AlertContext } from \"../../contexts/AlertContext\";\nimport { AuthContext } from \"../../contexts/AuthContext\";\nimport { Button, TextInput } f