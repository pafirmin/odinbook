messages":"187","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"188"},{"filePath":"189","messages":"190","errorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":"191","usedDeprecatedRules":"94"},{"filePath":"192","messages":"193","errorCount":0,"warningCount":2,"fixableErrorCount":0,"fixableWarningCount":0,"source":"194","usedDeprecatedRules":"94"},{"filePath":"195","messages":"196","errorCount":0,"warningCount":3,"fixableErrorCount":0,"fixableWarningCount":0,"source":"197","usedDeprecatedRules":"94"},{"filePath":"198","messages":"199","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"94"},"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/index.js",[],["200","201"],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/App.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/AuthReducer.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/auth/LogIn.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/auth/CreateAccount.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/contexts/AlertContext.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/contexts/AuthContext.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/layout/MainContent.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/layout/Header.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/NewPost.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/PostList.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/NewsFeed.js",["202"],"import axios from \"axios\";\nimport React, { useContext, useEffect, useState } from \"react\";\nimport { AuthContext } from \"../../contexts/AuthContext\";\nimport { LoadingContext } from \"../../contexts/LoadingContext\";\nimport NewPost from \"./NewPost\";\nimport PostList from \"./PostList\";\n\nconst NewsFeed = () => {\n  const { state } = useContext(AuthContext);\n  const { setLoading } = useContext(LoadingContext);\n\n  useEffect(() => {\n    document.title = \"Odinbook - Newsfeed\";\n  }, []);\n\n  const [posts, setPosts] = useState([]);\n  useEffect(() => {\n    const fetchPosts = async () => {\n      setLoading(true);\n      try {\n        const config = {\n          headers: {\n            Authorization: `bearer ${state.token}`,\n          },\n        };\n        const res = await axios.get(\"/api/posts/feed\", config);\n\n        setPosts(res.data);\n      } catch (err) {\n        console.error(err);\n      }\n      setLoading(false);\n    };\n    fetchPosts();\n  }, []);\n\n  return (\n    <div style={{ maxWidth: \"800px\", margin: \"auto\" }}>\n      <NewPost setPosts={setPosts} />\n      <PostList posts={posts} />\n    </div>\n  );\n};\n\nexport default NewsFeed;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/layout/UserPanel.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/socket/Socket.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/contexts/LoadingContext.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/search/SearchResults.js",["203"],"import React, { useEffect, useState } from \"react\";\nimport axios from \"axios\";\nimport { Link, useLocation } from \"react-router-dom\";\nimport UserCard from \"../users/UserCard\";\n\nconst useQuery = () => {\n  return new URLSearchParams(useLocation().search);\n};\n\nconst SearchResults = () => {\n  const query = useQuery();\n  const [results, setResults] = useState([]);\n\n  useEffect(() => {\n    document.title = `Search results - ${query.get(\"user\")}`;\n  }, [query]);\n\n  useEffect(() => {\n    const fetchResults = async () => {\n      try {\n        const res = await axios.get(`/api/search?user=${query.get(\"user\")}`);\n\n        setResults(res.data);\n      } catch (err) {\n        console.error(err);\n      }\n    };\n    fetchResults();\n  }, [query]);\n\n  return results.map((user) => <UserCard user={user} />);\n};\n\nexport default SearchResults;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/search/UserSearch.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/notifications/Notifications.js",["204"],"import React, { useContext, useEffect, useState } from \"react\";\nimport {\n  disconnectFromSocket,\n  listenForNotifications,\n} from \"../../socket/Socket\";\nimport axios from \"axios\";\nimport { AuthContext } from \"../../contexts/AuthContext\";\nimport { Notification, DropDown } from \"../utils/Utils\";\nimport NotificationListItem from \"./NotificationListItem\";\n\nconst Notifications = () => {\n  const [notifications, setNotifications] = useState([]);\n  const [showDropDown, setShowDropdown] = useState(false);\n  const [notificationCount, setNotificationCount] = useState(0);\n  const { authState } = useContext(AuthContext);\n\n  useEffect(() => {\n    listenForNotifications(setNotifications);\n\n    return () => disconnectFromSocket();\n  }, []);\n\n  useEffect(() => {\n    fetchNotifications();\n  }, []);\n\n  useEffect(() => {\n    setNotificationCount(\n      notifications.reduce(\n        (count, notification) => (notification.seen ? count : count + 1),\n        0\n      )\n    );\n  }, [notifications]);\n\n  const fetchNotifications = async () => {\n    try {\n      const config = {\n        headers: {\n          Authorization: `bearer ${authState.token}`,\n        },\n      };\n\n      const res = await axios.get(`/api/notifications`, config);\n\n      setNotifications(res.data);\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  const markNotificationsAsSeen = async () => {\n    try {\n      const config = {\n        headers: {\n          Authorization: `bearer ${authState.token}`,\n        },\n      };\n\n      const res = await axios.put(\"/api/notifications\", {}, config);\n\n      setTimeout(() => setNotificationCount(res.data), 3000);\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  const toggleDropDown = () => {\n    if (!showDropDown) markNotificationsAsSeen();\n\n    setShowDropdown(!showDropDown);\n  };\n\n  return (\n    <div style={{ position: \"relative\" }}>\n      <i\n        className=\"fas fa-bell\"\n        style={{ fontSize: \"1.2em\", cursor: \"pointer\" }}\n        onClick={toggleDropDown}\n      ></i>\n      {notificationCount > 0 && (\n        <Notification>{notificationCount}</Notification>\n      )}\n      <DropDown show={showDropDown}>\n        <h4>Notifications</h4>\n        <ul>\n          {notifications.map((notification) => (\n            <NotificationListItem\n              key={notification._id}\n              notification={notification}\n            />\n          ))}\n        </ul>\n        {!notifications.length && (\n          <div\n            style={{\n              color: \"#9d9d9d\",\n              marginTop: \"3em\",\n            }}\n          >\n            You have no notifications\n          </div>\n        )}\n      </DropDown>\n    </div>\n  );\n};\n\nexport default Notifications;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/notifications/NotificationListItem.js",["205","206"],"import React, { useEffect, useReducer, useState } from \"react\";\nimport { Link } from \"react-router-dom\";\nimport { formatDistanceToNow, parseISO } from \"date-fns\";\n\nconst notificationReducer = (state, action) => {\n  switch (action.type) {\n    case \"like\":\n      return { message: ` liked your post` };\n    case \"comment\":\n      return { message: ` commented on your post` };\n    case \"post\":\n      return { message: ` posted on your wall` };\n    default:\n      return state;\n  }\n};\n\nconst NotificationListItem = ({ notification }) => {\n  const { sender, post, type, date } = notification;\n  const initialState = { message: null };\n  const [state, dispatch] = useReducer(notificationReducer, initialState);\n  const [seen, setSeen] = useState(notification.seen);\n\n  useEffect(() => {\n    dispatch({ type });\n  }, []);\n\n  return (\n    <li onClick={() => setSeen(true)}>\n      <Link to={`/posts/${post}`}>\n        <div\n          style={{\n            display: \"flex\",\n            alignItems: \"center\",\n            borderBottom: \"1px solid #d9d9d9\",\n            borderRadius: \"6px\",\n            padding: \".4rem\",\n            gap: \".4rem\",\n            margin: \".2rem 0\",\n            backgroundColor: seen ? \"#fff\" : \"#e6efff\",\n          }}\n        >\n          <img className=\"small round thumbnail\" src={sender.profilePic} />\n          <div\n            style={{\n              display: \"flex\",\n              flexDirection: \"column\",\n              textAlign: \"left\",\n            }}\n          >\n            <span>\n              <span className=\"bold\">{sender.fullName}</span> {state.message}\n            </span>\n            <time style={{ color: \"#7a7a7a\" }}>\n              {formatDistanceToNow(parseISO(date), {\n                includeSeconds: true,\n                addSuffix: true,\n              })}\n            </time>\n          </div>\n        </div>\n      </Link>\n    </li>\n  );\n};\n\nexport default NotificationListItem;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/notifications/FriendRequests.js",["207"],"import React, { useContext, useEffect, useState } from \"react\";\nimport axios from \"axios\";\nimport { AuthContext } from \"../../contexts/AuthContext\";\nimport FriendRequestListItem from \"./FriendRequestListItem\";\nimport { disconnectFromSocket, listenForRequests } from \"../../socket/Socket\";\nimport { DropDown, Notification } from \"../utils/Utils\";\n\nconst FriendRequests = () => {\n  const [requests, setRequests] = useState([]);\n  const [showDropDown, setShowDropdown] = useState(false);\n  const { state } = useContext(AuthContext);\n\n  useEffect(() => {\n    fetchRequests();\n  }, [state.user]);\n\n  useEffect(() => {\n    listenForRequests(setRequests);\n\n    return () => disconnectFromSocket();\n  }, []);\n\n  const fetchRequests = async () => {\n    try {\n      const config = {\n        headers: {\n          Authorization: `bearer ${state.token}`,\n        },\n      };\n\n      const res = await axios.get(`/api/requests`, config);\n\n      setRequests(res.data);\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  const toggleDropDown = () => {\n    !showDropDown && fetchRequests();\n    setShowDropdown(!showDropDown);\n  };\n\n  return (\n    <div\n      style={{ position: \"relative\", display: \"flex\", alignItems: \"center\" }}\n    >\n      <div>\n        <i\n          style={{ fontSize: \"1.2em\", cursor: \"pointer\" }}\n          className=\"fas fa-user-friends\"\n          onClick={toggleDropDown}\n        />\n        {requests.length > 0 && <Notification>{requests.length}</Notification>}\n        <DropDown show={showDropDown}>\n          <span>Friend requests</span>\n          <ul>\n            {requests.map((request) => (\n              <FriendRequestListItem\n                key={request._id}\n                user={request.user}\n                requests={requests}\n                setRequests={setRequests}\n                requestID={request._id}\n              />\n            ))}\n          </ul>\n          {!requests.length && (\n            <div\n              style={{\n                color: \"#9d9d9d\",\n                marginTop: \"3em\",\n              }}\n            >\n              You have no pending friend requests\n            </div>\n          )}\n        </DropDown>\n      </div>\n    </div>\n  );\n};\n\nexport default FriendRequests;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/notifications/FriendRequestListItem.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/utils/Utils.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/utils/Alerts.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/utils/LoadingSpinner.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/utils/ScrollToTop.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/layout/MobileMenu.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/layout/MobilePanel.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/PostPage.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/hooks/useFriendshipStatus.js",["208"],"import { useEffect, useState } from \"react\";\n\nconst useFriendShipStatus = (currentUserID, user) => {\n  const [friendshipStatus, setFriendshipStatus] = useState(null);\n\n  useEffect(() => {\n    if (isCurrentUserProfile) {\n      setFriendshipStatus(\"isUser\");\n    } else if (userIsFriend) {\n      setFriendshipStatus(\"isFriend\");\n    } else if (requestIsPending) {\n      setFriendshipStatus(\"isPending\");\n    } else {\n      setFriendshipStatus(null);\n    }\n  }, [user]);\n\n  const isCurrentUserProfile = currentUserID === user._id;\n\n  const userIsFriend = user.friends.some(\n    (friend) =>\n      friend.user._id === currentUserID && friend.status === \"accepted\"\n  );\n\n  const requestIsPending = user.friends.some(\n    (friend) =>\n      friend.user._id === currentUserID && friend.status === \"recieved\"\n  );\n\n  return [friendshipStatus, setFriendshipStatus];\n};\n\nexport default useFriendShipStatus;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/UserCard.js",["209"],"import React from \"react\";\nimport { Link } from \"react-router-dom\";\n\nconst UserCard = ({ user }) => {\n  return (\n    <Link to={`/user/${user._id}`}>\n      <div\n        className=\"card\"\n        style={{ display: \"flex\", gap: \".5rem\", margin: \"8px 0\" }}\n      >\n        <img className=\"round thumbnail\" src={user.profilePic} />\n        <div\n          style={{\n            display: \"flex\",\n            flexDirection: \"column\",\n            justifyContent: \"center\",\n          }}\n        >\n          <h2 className=\"bold\">{user.fullName}</h2>\n          <span>{user.profile.location}</span>\n        </div>\n      </div>\n    </Link>\n  );\n};\n\nexport default UserCard;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/Friends.js",["210"],"import React, { useContext, useEffect, useState } from \"react\";\nimport axios from \"axios\";\nimport { useParams } from \"react-router-dom\";\nimport { AuthContext } from \"../../contexts/AuthContext\";\nimport UserCard from \"./UserCard\";\n\nconst Friends = () => {\n  const { state } = useContext(AuthContext);\n  const { userID } = useParams();\n  const [user, setUser] = useState(null);\n\n  useEffect(() => {\n    fetchUser();\n  }, []);\n\n  useEffect(() => {\n    if (user) {\n      document.title = `Odinbook - ${user.firstName}'s friends`;\n    }\n  });\n\n  const fetchUser = async () => {\n    const config = {\n      headers: {\n        Authorization: `bearer ${state.token}`,\n      },\n    };\n\n    const res = await axios.get(`/api/users/${userID}`, config);\n\n    setUser(res.data);\n  };\n\n  return (\n    <div>\n      {user && <h2>{user.fullName}'s friends</h2>}\n      {user && user.friends.map((friend) => <UserCard user={friend.user} />)}\n    </div>\n  );\n};\n\nexport default Friends;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/profile/UserPage.js",["211","212"],"import React, { useContext, useEffect, useState } from \"react\";\nimport axios from \"axios\";\nimport { useParams } from \"react-router-dom\";\nimport UserProfile from \"./UserProfile\";\nimport PostList from \"../../posts/PostList\";\nimport NewPost from \"../../posts/NewPost\";\nimport { LoadingContext } from \"../../../contexts/LoadingContext\";\nimport styled from \"styled-components\";\n\nconst UserPageWrapper = styled.div`\n  display: grid;\n  grid-template-columns: 1fr 2fr;\n  gap: 24px;\n\n  @media screen and (max-width: 800px) {\n    grid-template-columns: 1fr;\n  }\n`;\n\nconst UserPage = () => {\n  const { userID } = useParams();\n  const { setLoading } = useContext(LoadingContext);\n\n  const [user, setUser] = useState(null);\n  useEffect(() => {\n    const fetchUser = async () => {\n      setLoading(true);\n      try {\n        const res = await axios.get(`/api/users/${userID}`);\n\n        document.title = `Odinbook - ${res.data.fullName}`;\n        setUser(res.data);\n      } catch (err) {\n        console.error(err);\n      }\n    };\n    fetchUser();\n  }, [userID]);\n\n  const [posts, setPosts] = useState([]);\n  useEffect(() => {\n    const fetchPosts = async () => {\n      try {\n        const res = await axios.get(`/api/posts/user/${userID}/wall`);\n\n        setPosts(res.data);\n      } catch (err) {\n        console.error(err);\n      }\n      setLoading(false);\n    };\n    fetchPosts();\n  }, [userID]);\n\n  return (\n    <UserPageWrapper>\n      {user && <UserProfile user={user} />}\n      <div>\n        <NewPost setPosts={setPosts} userID={userID} />\n        <PostList posts={posts} />\n      </div>\n    </UserPageWrapper>\n  );\n};\n\nexport default UserPage;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/profile/EditProfile.js",["213","214"],"import React, { useContext, useState, useEffect } from \"react\";\nimport axios from \"axios\";\nimport { AuthContext } from \"../../../contexts/AuthContext\";\nimport { Button, TextInput } from \"../../utils/Utils\";\nimport { LoadingContext } from \"../../../contexts/LoadingContext\";\nimport { useHistory } from \"react-router-dom\";\nimport { AlertContext } from \"../../../contexts/AlertContext\";\n\nconst EditProfile = () => {\n  const history = useHistory();\n  const { token } = useContext(AuthContext).state;\n  const { setAlerts } = useContext(AlertContext);\n  const { setLoading } = useContext(LoadingContext);\n  const [image, setImage] = useState(\"\");\n  const [profileData, setProfileData] = useState({\n    location: \"\",\n    occupation: \"\",\n    bio: \"\",\n  });\n\n  useEffect(() => {\n    document.title = \"Edit Profile\";\n  }, []);\n\n  useEffect(() => {\n    const fetchProfile = async () => {\n      setLoading(true);\n      try {\n        const config = {\n          headers: {\n            \"Content-Type\": \"application/json\",\n            Authorization: `bearer ${token}`,\n          },\n        };\n        const res = await axios.get(`/api/users/profile`, config);\n        const profile = res.data;\n\n        setProfileData(profile);\n      } catch (err) {\n        console.error(err);\n      }\n      setLoading(false);\n    };\n    fetchProfile();\n  }, []);\n\n  const handleFile = (e) => {\n    const file = e.target.files[0];\n    const reader = new FileReader();\n    reader.readAsDataURL(file);\n    reader.onloadend = () => {\n      setImage(reader.result);\n    };\n  };\n\n  const handleChange = (e) => {\n    setProfileData({\n      ...profileData,\n      [e.target.name]: e.target.value,\n    });\n  };\n\n  const handleSubmit = async (e) => {\n    e.preventDefault();\n    setLoading(true);\n    try {\n      const config = {\n        headers: {\n          \"Content-Type\": \"application/json\",\n          Authorization: `bearer ${token}`,\n        },\n      };\n      const { location, occupation, bio } = profileData;\n      const body = JSON.stringify({ location, occupation, bio, image });\n\n      const res = await axios.put(`/api/users/profile`, body, config);\n      const userID = res.data;\n\n      history.push(`/user/${userID}`);\n    } catch (err) {\n      const errorArray = err.response.data.errors.map((err) => {\n        return { text: err.msg, type: \"danger\" };\n      });\n      setAlerts(errorArray);\n    }\n    setLoading(false);\n    setTimeout(() => setAlerts([]), 5000);\n  };\n\n  return (\n    <div>\n      <form className=\"user-form\" onSubmit={(e) => handleSubmit(e)}>\n        <header className=\"form-header\">\n          <h2>Edit your profile</h2>\n        </header>\n        <div className=\"form-group\">\n          <label htmlFor=\"location\">Location:</label>\n          <TextInput\n            id=\"location\"\n            name=\"location\"\n            value={profileData.location}\n            onChange={(e) => handleChange(e)}\n          />\n        </div>\n        <div className=\"form-group\">\n          <label htmlFor=\"occupation\">Occupation:</label>\n          <TextInput\n            id=\"occupation\"\n            name=\"occupation\"\n            value={profileData.occupation}\n            onChange={(e) => handleChange(e)}\n          />\n        </div>\n        <div className=\"form-group\">\n          <label htmlFor=\"bio\">A little about yourself:</label>\n          <TextInput\n            id=\"bio\"\n            name=\"bio\"\n            value={profileData.bio}\n            onChange={(e) => handleChange(e)}\n          />\n        </div>\n        <div className=\"form-group\">\n          <label htmlFor=\"file\">Profile pic:</label>\n          <input\n            type=\"file\"\n            id=\"file\"\n            name=\"file\"\n            onChange={(e) => handleFile(e)}\n          />\n        </div>\n        {image && <img src={image} className=\"large thumbnail\" />}\n        <Button>Save changes</Button>\n      </form>\n    </div>\n  );\n};\n\nexport default EditProfile;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/profile/UserProfile.js",["215"],"import React, { useEffect, useState } from \"react\";\nimport { useMediaQuery } from \"react-responsive\";\nimport styled from \"styled-components\";\nimport { sampleSize } from \"lodash\";\nimport { Link } from \"react-router-dom\";\nimport AddFriendBtn from \"./AddFriendBtn\";\nimport FriendCard from \"./FriendCard\";\n\nconst ProfileSection = styled.section`\n  padding: 0.8rem;\n  background: #fff;\n  border-radius: 8px;\n  box-shadow: 0px 1px 2px #9d9d9d;\n\n  & a:hover {\n    text-decoration: underline;\n  }\n`;\n\nconst ProfileHeader = styled.header`\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n`;\n\nconst ProfilePic = styled.div`\n  margin: auto auto 16px auto;\n  height: auto;\n  background-image: url(\"${({ src }) => src}\");\n  background-size: 100%;\n  background-repeat: no-repeat;\n  background-position: center;\n  width: 70%;\n  padding: 8px;\n  box-shadow: 2px 2px 8px #7d7d7d;\n\n  &:after {\n    content: \"\";\n    display: block;\n    padding-bottom: 100%;\n  }\n`;\n\nconst Profile = ({ user }) => {\n  const isMobile = useMediaQuery({ query: \"(max-width: 800px)\" });\n  const { location, bio, occupation } = user.profile;\n  const [friends, setFriends] = useState([]);\n\n  useEffect(() => {\n    if (!isMobile) {\n      const profileHeight = document.getElementById(\"profile\").scrollHeight;\n      const vh = window.innerHeight;\n      const stickyTop = vh - profileHeight;\n\n      document.getElementById(\"profile\").style.top = `${stickyTop}px`;\n    }\n  });\n\n  useEffect(() => {\n    const filteredFriends = user.friends.filter(\n      (friend) => friend.status === \"accepted\"\n    );\n    setFriends(sampleSize(filteredFriends, isMobile ? 6 : 9));\n  }, [user]);\n\n  return (\n    <div>\n      <div\n        id=\"profile\"\n        style={{\n          marginTop: \"16px\",\n          display: \"flex\",\n          flexDirection: \"column\",\n          gap: \"12px\",\n          position: \"sticky\",\n        }}\n      >\n        <ProfileSection>\n          <ProfilePic src={user.profilePic} />\n          <ProfileHeader>\n            <div>\n              <h2 className=\"bold\">{user.fullName}</h2>\n              <p>{location}</p>\n            </div>\n            <AddFriendBtn user={user} />\n          </ProfileHeader>\n          <div style={{ marginTop: \".8rem\" }}>\n            <div>\n              <h4>Occupation</h4>\n              <p>{occupation}</p>\n            </div>\n            <div style={{ marginTop: \".8rem\" }}>\n              <h4>Bio</h4>\n              <p>{bio}</p>\n            </div>\n          </div>\n        </ProfileSection>\n        <ProfileSection>\n          <ProfileHeader>\n            <h2 className=\"bold\">Friends</h2>\n            <span>\n              <Link to={`/users/${user._id}/friends`}>View all</Link>\n            </span>\n          </ProfileHeader>\n          <div\n            style={{\n              display: \"flex\",\n              justifyContent: \"space-between\",\n              flexWrap: \"wrap\",\n              marginTop: \"10px\",\n              gap: \"8px\",\n            }}\n          >\n            {friends.map((friend) => (\n              <FriendCard key={friend._id} friend={friend} />\n            ))}\n          </div>\n        </ProfileSection>\n        'Odinbook', created by Paul Firmin, 2021\n      </div>\n    </div>\n  );\n};\n\nexport default Profile;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/profile/FriendCard.js",["216"],"import React from \"react\";\nimport { Link } from \"react-router-dom\";\nconst FriendCard = ({ friend }) => {\n  return (\n    <div style={{ width: \"30%\", textAlign: \"center\" }}>\n      <Link to={`/user/${friend.user._id}`}>\n        <img className=\"large thumbnail\" src={friend.user.profilePic} />\n        <figcaption style={{ fontSize: \".85rem\", fontWeight: \"900\" }}>\n          {friend.user.fullName}\n        </figcaption>\n      </Link>\n    </div>\n  );\n};\n\nexport default FriendCard;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/profile/AddFriendBtn.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/post/Post.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/post/PostHeader.js",["217"],"import React from \"react\";\nimport { formatDistanceToNow, parseISO } from \"date-fns\";\nimport { Link } from \"react-router-dom\";\n\nconst PostHeader = ({ post }) => {\n  const { user, date, name, recipient, recipientName } = post;\n\n  return (\n    <div style={{ display: \"flex\", gap: \"8px\" }}>\n      <img className=\"small round thumbnail\" src={user.profilePic} />\n      <div\n        style={{\n          display: \"flex\",\n          flexDirection: \"column\",\n          justifyContent: \"space-between\",\n        }}\n      >\n        <Link to={`/user/${user._id}`}>\n          {recipient !== user._id ? (\n            <h3 style={{ fontSize: \"1.1em\" }}>\n              <span className=\"bold\">{name}</span> posted on{\" \"}\n              <span className=\"bold\">{recipientName}</span>'s wall\n            </h3>\n          ) : (\n            <h3 style={{ fontSize: \"1.2em\" }}>{name}</h3>\n          )}\n        </Link>\n        <time style={{ fontSize: \"0.8em\", color: \"#626262\" }}>\n          {formatDistanceToNow(parseISO(date), {\n            includeSeconds: true,\n            addSuffix: true,\n          })}\n        </time>\n      </div>\n    </div>\n  );\n};\n\nexport default PostHeader;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/post/comments/CommentSection.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/post/comments/Comment.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/post/comments/CommentForm.js",[],["218","219"],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/post/PostInteraction.js",["220"],"import React, { Fragment, useContext, useState, useEffect } from \"react\";\nimport axios from \"axios\";\nimport styled from \"styled-components\";\nimport { AuthContext } from \"../../../contexts/AuthContext\";\nimport { sendNotification } from \"../../../socket/Socket\";\n\nconst SocialDiv = styled.div`\n  display: flex;\n  justify-content: space-between;\n  border-bottom: 1px solid #c6c6c6;\n  padding: 0.2rem 0;\n`;\n\nconst SocialIcon = styled.i`\n  font-size: 1.3em;\n  color: #6b6b6b;\n  padding-right: 0.5rem;\n`;\n\nconst LikeThumb = styled(SocialIcon)`\n  color: ${({ isLiked }) => (isLiked ? \"#2d9ee9\" : \"#6b6b6b\")};\n\n  &:hover {\n    color: #2d9ee9;\n  }\n`;\n\nconst SocialBtn = styled.button`\n  border: none;\n  background: none;\n  font: inherit;\n  cursor: pointer;\n  width: 50%;\n  border-radius: 20px;\n  padding: 0.2rem 0;\n\n  &: hover {\n    background-color: #f5f5f5;\n  }\n`;\n\nconst PostInteraction = ({ post }) => {\n  const { authState } = useContext(AuthContext);\n  const { comments } = post;\n  const [likes, setLikes] = useState(post.likes);\n  const [isLiked, setIsLiked] = useState(false);\n\n  useEffect(() => {\n    setIsLiked(likes.map((like) => like.user._id).includes(authState.userID));\n  }, [likes]);\n\n  const handleLike = async () => {\n    try {\n      const config = {\n        headers: {\n          Authorization: `bearer ${authState.token}`,\n        },\n      };\n\n      const res = await axios.post(`/api/posts/${post._id}/like`, {}, config);\n      console.log(res.data);\n      setLikes(res.data);\n\n      if (!isLiked)\n        sendNotification({\n          sender: authState.userID,\n          recipient: post.user._id,\n          post: post._id,\n          type: \"like\",\n        });\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  const focusCommentBox = () => {\n    document.getElementById(`comment-box-${post._id}`).focus();\n  };\n\n  return (\n    <Fragment>\n      <SocialDiv>\n        <span>\n          {likes.length} like{likes.length !== 1 && \"s\"}\n        </span>\n        <span>\n          {comments.length} comment{comments.length !== 1 && \"s\"}\n        </span>\n      </SocialDiv>\n      <SocialDiv>\n        <SocialBtn onClick={handleLike}>\n          <LikeThumb isLiked={isLiked} className=\"fas fa-thumbs-up\" />\n          {isLiked ? \"You liked this\" : \"Like\"}\n        </SocialBtn>\n        <SocialBtn onClick={focusCommentBox}>\n          <SocialIcon className=\"far fa-comments\" />\n          Comment\n        </SocialBtn>\n      </SocialDiv>\n    </Fragment>\n  );\n};\n\nexport default PostInteraction;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/notifications/Messages.js",["221","222"],"import React, { useContext, useEffect, useState } from \"react\";\nimport axios from \"axios\";\nimport { AuthContext } from \"../../contexts/AuthContext\";\nimport { DropDown, Notification } from \"../utils/Utils\";\nimport MessagesListItem from \"./MessagesListItem\";\n\nconst Messages = () => {\n  const { authState } = useContext(AuthContext);\n  const [conversations, setConversations] = useState([]);\n  const [showDropDown, setShowDropdown] = useState(false);\n  const [unreadCount, setUnreadCount] = useState(0);\n  const [icon, setIcon] = useState(\"fas fa-envelope\");\n\n  useEffect(() => {\n    fetchConversations();\n  }, []);\n\n  useEffect(() => {\n    const unreadConvos = conversations.reduce(\n      (count, convo) => (convo.lastMessage.seen ? count : count + 1),\n      0\n    );\n    setUnreadCount(conversations.length);\n  }, [conversations]);\n\n  const fetchConversations = async () => {\n    try {\n      const config = {\n        headers: {\n          Authorization: `bearer ${authState.token}`,\n        },\n      };\n\n      const res = await axios.get(`/api/messaging`, config);\n\n      setConversations(res.data);\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  const toggleDropDown = () => {\n    !showDropDown && fetchConversations();\n    setIcon(showDropDown ? \"fas fa-envelope\" : \"fas fa-envelope-open\");\n    setShowDropdown(!showDropDown);\n  };\n\n  const getParticipant = (participants) => {\n    return participants.find((user) => user._id !== authState.userID);\n  };\n\n  return (\n    <div\n      style={{ position: \"relative\", display: \"flex\", alignItems: \"center\" }}\n    >\n      <div>\n        <i\n          style={{ fontSize: \"1.2em\", cursor: \"pointer\" }}\n          className={icon}\n          onClick={toggleDropDown}\n        />\n        {unreadCount > 0 && <Notification>{unreadCount}</Notification>}\n        <DropDown show={showDropDown} style={{ overflow: \"visible\" }}>\n          <ul>\n            {conversations.map((convo) => (\n              <MessagesListItem\n                convo={convo}\n                participant={getParticipant(convo.participants)}\n              />\n            ))}\n          </ul>\n        </DropDown>\n      </div>\n    </div>\n  );\n};\n\nexport default Messages;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/chat/Conversation.js",["223","224","225"],"import React, { useContext, useEffect, useState, useRef } from \"react\";\nimport axios from \"axios\";\nimport { AuthContext } from \"../../contexts/AuthContext\";\nimport { Button, TextInput } from \"../utils/Utils\";\nimport {\n  disconnectFromSocket,\n  listenForMessages,\n  sendMessage,\n} from \"../../socket/Socket\";\n\nconst Conversation = ({ conversation, participant, setLastMessage }) => {\n  const { authState } = useContext(AuthContext);\n  const [messages, setMessages] = useState([]);\n  const 