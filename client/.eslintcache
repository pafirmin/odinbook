ork/client/src/contexts/AlertContext.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/contexts/AuthContext.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/layout/MainContent.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/layout/Header.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/NewPost.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/PostList.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/NewsFeed.js",["180"],"import axios from \"axios\";\nimport React, { useContext, useEffect, useState } from \"react\";\nimport { AuthContext } from \"../../contexts/AuthContext\";\nimport { LoadingContext } from \"../../contexts/LoadingContext\";\nimport NewPost from \"./NewPost\";\nimport PostList from \"./PostList\";\n\nconst NewsFeed = () => {\n  const { state } = useContext(AuthContext);\n  const { setLoading } = useContext(LoadingContext);\n\n  useEffect(() => {\n    document.title = \"Odinbook - Newsfeed\";\n  }, []);\n\n  const [posts, setPosts] = useState([]);\n  useEffect(() => {\n    const fetchPosts = async () => {\n      setLoading(true);\n      try {\n        const config = {\n          headers: {\n            Authorization: `bearer ${state.token}`,\n          },\n        };\n        const res = await axios.get(\"/api/posts/feed\", config);\n\n        setPosts(res.data);\n      } catch (err) {\n        console.error(err);\n      }\n      setLoading(false);\n    };\n    fetchPosts();\n  }, []);\n\n  return (\n    <div style={{ maxWidth: \"800px\", margin: \"auto\" }}>\n      <NewPost setPosts={setPosts} />\n      <PostList posts={posts} />\n    </div>\n  );\n};\n\nexport default NewsFeed;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/layout/UserPanel.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/socket/Socket.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/contexts/LoadingContext.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/search/SearchResults.js",["181"],"import React, { useEffect, useState } from \"react\";\nimport axios from \"axios\";\nimport { Link, useLocation } from \"react-router-dom\";\nimport UserCard from \"../users/UserCard\";\n\nconst useQuery = () => {\n  return new URLSearchParams(useLocation().search);\n};\n\nconst SearchResults = () => {\n  const query = useQuery();\n  const [results, setResults] = useState([]);\n\n  useEffect(() => {\n    document.title = `Search results - ${query.get(\"user\")}`;\n  }, [query]);\n\n  useEffect(() => {\n    const fetchResults = async () => {\n      try {\n        const res = await axios.get(`/api/search?user=${query.get(\"user\")}`);\n\n        setResults(res.data);\n      } catch (err) {\n        console.error(err);\n      }\n    };\n    fetchResults();\n  }, [query]);\n\n  return results.map((user) => <UserCard user={user} />);\n};\n\nexport default SearchResults;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/search/UserSearch.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/notifications/Notifications.js",["182","183"],"import React, { useContext, useEffect, useState } from \"react\";\nimport {\n  disconnectFromSocket,\n  listenForNotifications,\n} from \"../../socket/Socket\";\nimport axios from \"axios\";\nimport { AuthContext } from \"../../contexts/AuthContext\";\nimport { Notification, DropDown } from \"../utils/Utils\";\nimport NotificationListItem from \"./NotificationListItem\";\n\nconst Notifications = () => {\n  const [notifications, setNotifications] = useState([]);\n  const [showDropDown, setShowDropdown] = useState(false);\n  const [notificationCount, setNotificationCount] = useState(0);\n  const { state } = useContext(AuthContext);\n\n  useEffect(() => {\n    listenForNotifications(fetchNotifications);\n\n    return () => disconnectFromSocket();\n  }, []);\n\n  useEffect(() => {\n    fetchNotifications();\n  }, []);\n\n  useEffect(() => {\n    setNotificationCount(\n      notifications.reduce(\n        (count, notification) => (notification.seen ? count : count + 1),\n        0\n      )\n    );\n  }, [notifications]);\n\n  const fetchNotifications = async () => {\n    try {\n      const config = {\n        headers: {\n          Authorization: `bearer ${state.token}`,\n        },\n      };\n\n      const res = await axios.get(`/api/notifications`, config);\n\n      setNotifications(res.data);\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  const markNotificationsAsSeen = async () => {\n    try {\n      const config = {\n        headers: {\n          Authorization: `bearer ${state.token}`,\n        },\n      };\n\n      const res = await axios.put(\"/api/notifications\", {}, config);\n\n      setTimeout(() => setNotificationCount(res.data), 3000);\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  const toggleDropDown = () => {\n    if (!showDropDown) markNotificationsAsSeen();\n\n    setShowDropdown(!showDropDown);\n  };\n\n  return (\n    <div style={{ position: \"relative\" }}>\n      <i\n        className=\"fas fa-bell\"\n        style={{ fontSize: \"1.2em\", cursor: \"pointer\" }}\n        onClick={toggleDropDown}\n      ></i>\n      {notificationCount > 0 && (\n        <Notification>{notificationCount}</Notification>\n      )}\n      <DropDown show={showDropDown}>\n        <h4>Notifications</h4>\n        <ul>\n          {notifications.map((notification) => (\n            <NotificationListItem\n              key={notification._id}\n              notification={notification}\n            />\n          ))}\n        </ul>\n        {!notifications.length && (\n          <div\n            style={{\n              color: \"#9d9d9d\",\n              marginTop: \"3em\",\n            }}\n          >\n            You have no notifications\n          </div>\n        )}\n      </DropDown>\n    </div>\n  );\n};\n\nexport default Notifications;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/notifications/NotificationListItem.js",["184","185"],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/notifications/FriendRequests.js",["186"],"import React, { useContext, useEffect, useState } from \"react\";\nimport axios from \"axios\";\nimport { AuthContext } from \"../../contexts/AuthContext\";\nimport FriendRequestListItem from \"./FriendRequestListItem\";\nimport { disconnectFromSocket, listenForRequests } from \"../../socket/Socket\";\nimport { DropDown, Notification } from \"../utils/Utils\";\n\nconst FriendRequests = () => {\n  const [requests, setRequests] = useState([]);\n  const [showDropDown, setShowDropdown] = useState(false);\n  const { state } = useContext(AuthContext);\n\n  useEffect(() => {\n    fetchRequests();\n  }, [state.user]);\n\n  useEffect(() => {\n    listenForRequests(setRequests);\n\n    return () => disconnectFromSocket();\n  }, []);\n\n  const fetchRequests = async () => {\n    try {\n      const config = {\n        headers: {\n          Authorization: `bearer ${state.token}`,\n        },\n      };\n\n      const res = await axios.get(`/api/requests`, config);\n\n      setRequests(res.data);\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  const toggleDropDown = () => {\n    !showDropDown && fetchRequests();\n    setShowDropdown(!showDropDown);\n  };\n\n  return (\n    <div\n      style={{ position: \"relative\", display: \"flex\", alignItems: \"center\" }}\n    >\n      <div>\n        <i\n          style={{ fontSize: \"1.2em\", cursor: \"pointer\" }}\n          className=\"fas fa-user-friends\"\n          onClick={toggleDropDown}\n        />\n        {requests.length > 0 && <Notification>{requests.length}</Notification>}\n        <DropDown show={showDropDown}>\n          <span>Friend requests</span>\n          <ul>\n            {requests.map((request) => (\n              <FriendRequestListItem\n                key={request._id}\n                user={request.user}\n                requests={requests}\n                setRequests={setRequests}\n                requestID={request._id}\n              />\n            ))}\n          </ul>\n          {!requests.length && (\n            <div\n              style={{\n                color: \"#9d9d9d\",\n                marginTop: \"3em\",\n              }}\n            >\n              You have no pending friend requests\n            </div>\n          )}\n        </DropDown>\n      </div>\n    </div>\n  );\n};\n\nexport default FriendRequests;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/notifications/FriendRequestListItem.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/utils/Utils.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/utils/Alerts.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/utils/LoadingSpinner.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/utils/ScrollToTop.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/layout/MobileMenu.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/layout/MobilePanel.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/PostPage.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/hooks/useFriendshipStatus.js",["187"],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/UserCard.js",["188"],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/compo