r();\n  }, [userID]);\n\n  const [posts, setPosts] = useState([]);\n  useEffect(() => {\n    const fetchPosts = async () => {\n      try {\n        const res = await axios.get(`/api/posts/user/${userID}/wall`);\n\n        setPosts(res.data);\n      } catch (err) {\n        console.error(err);\n      }\n      setLoading(false);\n    };\n    fetchPosts();\n  }, [userID]);\n\n  return (\n    <UserPageWrapper>\n      {user && <UserProfile user={user} />}\n      <div>\n        <NewPost setPosts={setPosts} userID={userID} />\n        <PostList posts={posts} />\n      </div>\n    </UserPageWrapper>\n  );\n};\n\nexport default UserPage;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/NewPost.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/PostList.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/profile/UserProfile.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/Post.js",["166","167"],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/comments/CommentSection.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/comments/CommentForm.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/comments/Comment.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/NewsFeed.js",["168"],"import axios from \"axios\";\nimport React, { useContext, useEffect, useState } from \"react\";\nimport { AuthContext } from \"../../contexts/AuthContext\";\nimport { LoadingContext } from \"../../contexts/LoadingContext\";\nimport NewPost from \"./NewPost\";\nimport PostList from \"./PostList\";\n\nconst NewsFeed = () => {\n  const { state } = useContext(AuthContext);\n  const { setLoading } = useContext(LoadingContext);\n\n  useEffect(() => {\n    document.title = \"Odinbook - Newsfeed\";\n  }, []);\n\n  const [posts, setPosts] = useState([]);\n  useEffect(() => {\n    const fetchPosts = async () => {\n      setLoading(true);\n      try {\n        const config = {\n          headers: {\n            Authorization: `bearer ${state.token}`,\n          },\n        };\n        const res = await axios.get(\"/api/posts/feed\", config);\n\n        setPosts(res.data);\n      } catch (err) {\n        console.error(err);\n      }\n      setLoading(false);\n    };\n    fetchPosts();\n  }, []);\n\n  return (\n    <div style={{ maxWidth: \"800px\", margin: \"auto\" }}>\n      <NewPost setPosts={setPosts} />\n      <PostList posts={posts} />\n    </div>\n  );\n};\n\nexport default NewsFeed;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/layout/UserPanel.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/socket/Socket.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/profile/FriendCard.js",["169"],"import React from \"react\";\nimport { Link } from \"react-router-dom\";\nconst FriendCard = ({ friend }) => {\n  return (\n    <div style={{ width: \"30%\", textAlign: \"center\" }}>\n      <Link to={`/user/${friend.user._id}`}>\n        <img className=\"large thumbnail\" src={friend.user.profilePic} />\n        <figcaption style={{ fontSize: \".85rem\", fontWeight: \"900\" }}>\n          {friend.user.fullName}\n        </figcaption>\n      </Link>\n    </div>\n  );\n};\n\nexport default FriendCard;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/contexts/LoadingContext.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/profile/EditProfile.js",["170","171"],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/search/SearchResults.js",["172"],"import React, { useEffect, useState } from \"react\";\nimport axios from \"axios\";\nimport { Link, useLocation } from \"react-router-dom\";\nimport UserResult from \"./UserResult\";\n\nconst useQuery = () => {\n  return new URLSearchParams(useLocation().search);\n};\n\nconst SearchResults = () => {\n  const query = useQuery();\n  const [results, setResults] = useState([]);\n\n  useEffect(() => {\n    document.title = `Search results - ${query.get(\"user\")}`;\n  }, [query]);\n\n  useEffect(() => {\n    const fetchResults = async () => {\n      try {\n        const res = await axios.get(`/api/search?user=${query.get(\"user\")}`);\n\n        setResults(res.data);\n      } catch (err) {\n        console.error(err);\n      }\n    };\n    fetchResults();\n  }, [query]);\n\n  return (\n    <div>\n      {results.map((user) => (\n        <UserResult user={user} />\n      ))}\n    </div>\n  );\n};\n\nexport default SearchResults;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/search/UserSearch.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/notifications/Notifications.js",["173","174"],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/notifications/NotificationListItem.js",["175","176"],"import React, { useEffect, useReducer } from \"react\";\nimport { Link } from \"react-router-dom\";\nimport { formatDistanceToNow, parseISO } from \"date-fns\";\n\nconst notificationReducer = (state, action) => {\n  switch (action.type) {\n    case \"like\":\n      return { message: ` liked your post` };\n    case \"comment\":\n      return { message: ` commented on your post` };\n    case \"post\":\n      return { message: ` posted on your wall` };\n    default:\n      return state;\n  }\n};\n\nconst NotificationListItem = ({ notification }) => {\n  const { sender, post, type, date, seen } = notification;\n  const initialState = { message: null };\n  const [state, dispatch] = useReducer(notificationReducer, initialState);\n\n  useEffect(() => {\n    dispatch({ type });\n  }, []);\n\n  return (\n    <li>\n      <Link to={`/posts/${post}`}>\n        <div\n          style={{\n            display: \"flex\",\n            alignItems: \"center\",\n            borderBottom: \"1px solid #d9d9d9\",\n            padding: \".4rem 0\",\n            gap: \".4rem\",\n          }}\n        >\n          <img className=\"small round thumbnail\" src={sender.profilePic} />\n          <div\n            style={{\n              display: \"flex\",\n              flexDirection: \"column\",\n              textAlign: \"left\",\n              backgroundColor: seen ? \"#fff\" : \"#c3c3c3\",\n            }}\n          >\n            {sender.fullName + state.message}\n            <time style={{ color: \"#7a7a7a\" }}>\n              {formatDistanceToNow(parseISO(date), {\n                includeSeconds: true,\n                addSuffix: true,\n              })}\n            </time>\n          </div>\n        </div>\n      </Link>\n    </li>\n  );\n};\n\nexport default NotificationListItem;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/notifications/FriendRequests.js",["177"],"import React, { useContext, useEffect, useState } from \"react\";\nimport axios from \"axios\";\nimport { AuthContext } from \"../../contexts/AuthContext\";\nimport FriendRequestListItem from \"./FriendRequestListItem\";\nimport { disconnectFromSocket, listenForRequests } from \"../../socket/Socket\";\nimport { DropDown, Notification } from \"../utils/Utils\";\n\nconst FriendRequests = () => {\n  const [requests, setRequests] = useState([]);\n  const [showDropDown, setShowDropdown] = useState(false);\n  const { state } = useContext(AuthContext);\n\n  useEffect(() => {\n    fetchRequests();\n  }, [state.user]);\n\n  useEffect(() => {\n    listenForRequests(setRequests);\n\n    return () => disconnectFromSocket();\n  }, []);\n\n  const fetchRequests = async () => {\n    try {\n      const config = {\n        headers: {\n          Authorization: `bearer ${state.token}`,\n        },\n      };\n\n      const res = await axios.get(`/api/requests`, config);\n\n      setRequests(res.data);\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  const toggleDropDown = () => {\n    !showDropDown && fetchRequests();\n    setShowDropdown(!showDropDown);\n  };\n\n  return (\n    <div\n      style={{ position: \"relative\", display: \"flex\", alignItems: \"center\" }}\n    >\n      <div>\n        <i\n          style={{ fontSize: \"1.2em\", cursor: \"pointer\" }}\n          className=\"fas fa-user-friends\"\n          onClick={toggleDropDown}\n        />\n        {requests.length > 0 && <Notification>{requests.length}</Notification>}\n        <DropDown show={showDropDown}>\n          <span>Friend requests</span>\n          <ul>\n            {requests.map((request) => (\n              <FriendRequestListItem\n                key={request._id}\n                user={request.user}\n                requests={requests}\n                setRequests={setRequests}\n                requestID={request._id}\n              />\n            ))}\n          </ul>\n          {!requests.length && (\n            <div\n              style={{\n                color: \"#9d9d9d\",\n                marginTop: \"3em\",\n              }}\n            >\n              You have no pending friend requests\n            </div>\n          )}\n        </DropDown>\n      </div>\n    </div>\n  );\n};\n\nexport default FriendRequests;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/notifications/FriendRequestListItem.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/utils/Utils.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/utils/Alerts.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/utils/LoadingSpinner.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/utils/ScrollToTop.js",[],"/