User] = useState(null);\n  useEffect(() => {\n    const fetchUser = async () => {\n      setLoading(true);\n      try {\n        const res = await axios.get(`/api/users/${userID}`);\n\n        document.title = `Odinbook - ${res.data.fullName}`;\n        setUser(res.data);\n      } catch (err) {\n        console.error(err);\n      }\n    };\n    fetchUser();\n  }, [userID]);\n\n  const [posts, setPosts] = useState([]);\n  useEffect(() => {\n    const fetchPosts = async () => {\n      try {\n        const res = await axios.get(`/api/posts/user/${userID}/wall`);\n\n        setPosts(res.data);\n      } catch (err) {\n        console.error(err);\n      }\n      setLoading(false);\n    };\n    fetchPosts();\n  }, [userID]);\n\n  return (\n    <UserPageWrapper>\n      {user && <UserProfile user={user} />}\n      <div>\n        <NewPost setPosts={setPosts} userID={userID} />\n        <PostList posts={posts} />\n      </div>\n    </UserPageWrapper>\n  );\n};\n\nexport default UserPage;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/NewPost.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/PostList.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/profile/UserProfile.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/Post.js",["170","171"],"import React, { useContext, useEffect, useState } from \"react\";\nimport { formatDistanceToNow, parseISO } from \"date-fns\";\nimport { Link } from \"react-router-dom\";\nimport styled from \"styled-components\";\nimport axios from \"axios\";\nimport { AuthContext } from \"../../contexts/AuthContext\";\nimport Comments from \"../comments/CommentSection\";\nimport { sendNotification } from \"../../socket/Socket\";\n\nconst PostContainer = styled.div`\n  box-shadow: 0px 1px 2px #9d9d9d;\n  margin: 16px auto;\n  padding: 0.8rem;\n  border-radius: 8px;\n  background-color: #fff;\n\n  @media (max-width: 700px) {\n    border-radius: 0;\n  }\n`;\n\nconst SocialDiv = styled.div`\n  display: flex;\n  justify-content: space-between;\n  border-bottom: 1px solid #c6c6c6;\n  padding: 0.2rem 0;\n`;\n\nconst SocialIcon = styled.i`\n  font-size: 1.3em;\n  color: #6b6b6b;\n  padding-right: 0.5rem;\n`;\n\nconst LikeThumb = styled(SocialIcon)`\n  color: ${(props) => (props.isLiked ? \"#2d9ee9\" : \"#6b6b6b\")};\n\n  &:hover {\n    color: #2d9ee9;\n  }\n`;\n\nconst SocialBtn = styled.button`\n  border: none;\n  background: none;\n  font: inherit;\n  cursor: pointer;\n  width: 50%;\n  border-radius: 20px;\n  padding: 0.2rem 0;\n\n  &: hover {\n    background-color: #f5f5f5;\n  }\n`;\n\nconst Post = ({ post }) => {\n  const { user, date, name, recipient, recipientName, text, profilePic } = post;\n  const { state } = useContext(AuthContext);\n  const [likes, setLikes] = useState(post.likes);\n  const [comments, setComments] = useState(post.comments);\n  const [isLiked, setIsLiked] = useState(false);\n\n  useEffect(() => {\n    setIsLiked(likes.map((like) => like.user._id).includes(state.userID));\n  }, [likes]);\n\n  const handleLike = async () => {\n    try {\n      const config = {\n        headers: {\n          Authorization: `bearer ${state.token}`,\n        },\n      };\n      const res = await axios.post(`/api/posts/${post._id}/like`, {}, config);\n\n      setLikes(res.data);\n      setIsLiked(!isLiked);\n\n      if (!isLiked)\n        sendNotification({\n          sender: state.userID,\n          recipientID: post.user._id,\n          post: post._id,\n          type: \"like\",\n        });\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  const focusCommentBox = () => {\n    document.getElementById(`comment-box-${post._id}`).focus();\n  };\n\n  return (\n    <PostContainer>\n      <div style={{ display: \"flex\", gap: \"8px\" }}>\n        <img className=\"small round thumbnail\" src={profilePic} />\n        <div\n          style={{\n            display: \"flex\",\n            flexDirection: \"column\",\n            justifyContent: \"space-between\",\n          }}\n        >\n          <Link to={`/user/${user._id}`}>\n            {recipient !== user._id ? (\n              <h3 style={{ fontSize: \"1.1em\" }}>\n                <span className=\"bold\">{name}</span> posted on{\" \"}\n                <span className=\"bold\">{recipientName}</span>'s wall\n              </h3>\n            ) : (\n              <h3 style={{ fontSize: \"1.2em\" }}>{name}</h3>\n            )}\n          </Link>\n          <time style={{ fontSize: \"0.8em\", color: \"#626262\" }}>\n            {formatDistanceToNow(parseISO(date), {\n              includeSeconds: true,\n              addSuffix: true,\n            })}\n          </time>\n        </div>\n      </div>\n      <div style={{ margin: \".8rem 0\" }}>{text}</div>\n      <SocialDiv>\n        <span>\n          {likes.length} like{likes.length !== 1 && \"s\"}\n        </span>\n        <span>\n          {comments.length} comment{comments.length !== 1 && \"s\"}\n        </span>\n      </SocialDiv>\n      <SocialDiv>\n        <SocialBtn onClick={handleLike}>\n          <LikeThumb isLiked={isLiked} className=\"fas fa-thumbs-up\" />\n          {isLiked ? \"You liked this\" : \"Like\"}\n        </SocialBtn>\n        <SocialBtn onClick={focusCommentBox}>\n          <SocialIcon className=\"far fa-comments\" />\n          Comment\n        </SocialBtn>\n      </SocialDiv>\n      <Comments comments={comments} setComments={setComments} post={post} />\n    </PostContainer>\n  );\n};\n\nexport default Post;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/comments/CommentSection.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/comments/CommentForm.js",[],["172","173"],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/comments/Comment.js",[],"/home/paul/Documents/the