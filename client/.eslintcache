40px;\n  background-image: url(\"${({ url }) => url}\");\n  background-size: 100%;\n  border-radius: 50%;\n`;\n\nconst Post = ({ post }) => {\n  const { state } = useContext(AuthContext);\n  const [likes, setLikes] = useState(post.likes);\n  const [isLiked, setIsLiked] = useState(false);\n  const [comments, setComments] = useState(post.comments);\n\n  useEffect(() => {\n    setIsLiked(likes.map((like) => like.user._id).includes(state.userID));\n  }, []);\n\n  const handleLike = async () => {\n    try {\n      const config = {\n        headers: {\n          Authorization: `bearer ${state.token}`,\n        },\n      };\n      const res = await axios.post(`/api/posts/${post._id}/like`, {}, config);\n\n      setLikes(res.data);\n      setIsLiked(!isLiked);\n\n      if (!isLiked)\n        sendNotification({\n          sender: state.userID,\n          recipientID: post.user._id,\n          post: post._id,\n          type: \"like\",\n        });\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  const focusCommentBox = () => {\n    document.getElementById(`comment-box-${post._id}`).focus();\n  };\n\n  return (\n    <PostContainer>\n      <div style={{ display: \"flex\", gap: \"8px\" }}>\n        <ProfilePic url={post.user.profilePic} />\n        <div\n          style={{\n            display: \"flex\",\n            flexDirection: \"column\",\n            justifyContent: \"space-between\",\n          }}\n        >\n          <Link to={`/user/${post.user._id}`}>\n            <h3 style={{ fontSize: \"1.2em\" }}>{post.name}</h3>\n          </Link>\n          <time style={{ fontSize: \"0.8em\", color: \"#626262\" }}>\n            {moment(post.date).fromNow()}\n          </time>\n        </div>\n      </div>\n      <div style={{ margin: \".8rem 0\" }}>{post.text}</div>\n      <SocialDiv>\n        <span>\n          {likes.length} like{likes.length !== 1 && \"s\"}\n        </span>\n        <span>\n          {comments.length} comment{comments.length !== 1 && \"s\"}\n        </span>\n      </SocialDiv>\n      <SocialDiv>\n        <SocialBtn onClick={handleLike}>\n          <LikeThumb isLiked={isLiked} className=\"fas fa-thumbs-up\" />\n          {isLiked ? \"You liked this\" : \"Like\"}\n        </SocialBtn>\n        <SocialBtn onClick={focusCommentBox}>\n          <SocialIcon className=\"far fa-comments\" />\n          Comment\n        </SocialBtn>\n      </SocialDiv>\n      <Comments comments={comments} setComments={setComments} post={post} />\n    </PostContainer>\n  );\n};\n\nexport default Post;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/comments/CommentSection.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/comments/CommentForm.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/comments/Comment.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/NewsFeed.js",["128"],"import axios from \"axios\";\nimport styled from \"styled-components\";\nimport React, { useContext, useEffect, useState } from \"react\";\nimport { AuthContext } from \"../../contexts/AuthContext\";\nimport NewPost from \"./NewPost\";\nimport PostList from \"./PostList\";\nimport { Button } from \"../Utils\";\n\nconst NewPostBtn = styled(Button)`\n  border-radius: 20px;\n  width: 70%;\n  margin: auto;\n\n  &:hover {\n    background-color: #2883bf;\n  }\n`;\n\nconst NewsFeed = () => {\n  const [posts, setPosts] = useState([]);\n  const [formShow, setFormShow] = useState(false);\n  const { state } = useContext(AuthContext);\n\n  useEffect(() => {\n    const fetchPosts = async () => {\n      try {\n        const config = {\n          headers: {\n            Authorization: `bearer ${state.token}`,\n          },\n        };\n        const res = await axios.get(\"/api/posts/feed\", config);\n\n        setPosts(res.data);\n      } catch (err) {\n        console.error(err);\n      }\n    };\n    fetchPosts();\n  }, []);\n\n  const toggleForm = () => {\n    setFormShow(!formShow);\n  };\n\n  return (\n    <div>\n      <div style={{ textAlign: \"center\" }}>\n        <NewPostBtn onClick={toggleForm}>Post something...</NewPostBtn>\n      </div>\n      <NewPost posts={posts} setPosts={setPosts} formShow={formShow} />\n      <PostList posts={posts} />\n    </div>\n  );\n};\n\nexport default NewsFeed;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/layout/UserPanel.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/FriendRequests.js",["129"],"import React, { useContext, useEffect, useState } from \"react\";\nimport axios from \"axios\";\nimport { AuthContext } from \"../contexts/AuthContext\";\nimport FriendRequestListItem from \"./FriendRequestListItem\";\nimport { disconnectFromSocket, listenForRequests } from \"../socket/Socket\";\nimport { DropDown, Notification } from \"./Utils\";\n\nconst FriendRequests = () => {\n  const [requests, setRequests] = useState([]);\n  const [showDropDown, setShowDropdown] = useState(false);\n  const { state } = useContext(AuthContext);\n\n  useEffect(() => {\n    fetchRequests();\n  }, [state.user]);\n\n  useEffect(() => {\n    listenForRequests(setRequests);\n\n    return () => disconnectFromSocket();\n  }, []);\n\n  const fetchRequests = async () => {\n    try {\n      const config = {\n        headers: {\n          Authorization: `bearer ${state.token}`,\n        },\n      };\n\n      const res = await axios.get(`/api/requests`, config);\n\n      setRequests(res.data);\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  const toggleDropDown = () => {\n    !showDropDown && fetchRequests();\n    setShowDropdown(!showDropDown);\n  };\n\n  return (\n    <div\n      style={{ position: \"relative\", display: \"flex\", alignItems: \"center\" }}\n    >\n      <div>\n        <i\n          style={{ fontSize: \"1.2em\", cursor: \"pointer\" }}\n          className=\"fas fa-user-friends\"\n          onClick={toggleDropDown}\n        />\n        {requests.length > 0 && <Notification>{requests.length}</Notification>}\n        <DropDown show={showDropDown}>\n          <span>Friend requests</span>\n          <ul>\n            {requests.map((request) => (\n              <FriendRequestListItem\n                key={request._id}\n                user={request.user}\n                requests={requests}\n                setRequests={setRequests}\n                requestID={request._id}\n              />\n            ))}\n          </ul>\n          {!requests.length && (\n            <div\n              style={{\n                color: \"#9d9d9d\",\n                marginTop: \"3em\",\n              }}\n            >\n              You have no pending friend requests\n            </div>\n          )}\n        </DropDown>\n      </div>\n    </div>\n  );\n};\n\nexport default FriendRequests;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/FriendRequestListItem.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/SearchResults.js",["130"],"import React, { useEffect, useState } from \"react\";\nimport axios from \"axios\";\nimport { Link, useLocation } from \"react-router-dom\";\n\nconst useQuery = () => {\n  return new URLSearchParams(useLocation().search);\n};\n\nconst SearchResults = () => {\n  const query = useQuery();\n  const [results, setResults] = useState([]);\n\n  useEffect(() => {\n    const fetchResults = async () => {\n      try {\n        const res = await axios.get(`/api/search?user=${query.get(\"user\")}`);\n\n        setResults(res.data);\n      } catch (err) {\n        console.error(err);\n      }\n    };\n    fetchResults();\n  }, []);\n\n  return (\n    <div>\n      {results.map((user) => (\n        <p key={user._id}>\n          <Link to={`/user/${user._id}`}>{user.fullName}</Link>\n        </p>\n      ))}\n    </div>\n  );\n};\n\nexpor