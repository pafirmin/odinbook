radius: 20px;\n  padding: 1rem;\n  margin: 8px 6px 8px 0;\n  width: 66%;\n  align-self: ${({ sender, user }) =>\n    sender === user ? \"flex-end\" : \"flex-start\"};\n`;\n\nconst TimeStamp = styled.time`\n  position: absolute;\n  top: 0px;\n  left: 50%;\n  transform: translate(-50%);\n  font-size: 0.7rem;\n  color: #525252;\n  width: 100%;\n  text-align: center;\n`;\n\nconst ChatMessage = ({ message }) => {\n  const { authState } = useContext(AuthContext);\n  return (\n    <MessageWrapper sender={message.sender._id} user={authState.userID}>\n      <TimeStamp title={format(parseISO(message.date), \"PPPppp\")}>\n        {formatDistanceToNow(parseISO(message.date), {\n          includeSeconds: true,\n          addSuffix: true,\n        })}\n      </TimeStamp>\n      <div\n        style={{\n          display: \"flex\",\n          alignContent: \"space-between\",\n          gap: \".3rem\",\n        }}\n      >\n        <img className=\"tiny round thumbnail\" src={message.sender.profilePic} />{\" \"}\n        {message.text}\n      </div>{\" \"}\n    </MessageWrapper>\n  );\n};\n\nexport default ChatMessage;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/profile/MessageForm.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/profile/ProfileTop.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/profile/ProfileDetails.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/profile/ProfileFriends.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/UserPage.js",["252","253"],"import React, { useContext, useEffect, useState } from \"react\";\nimport axios from \"axios\";\nimport { useParams } from \"react-router-dom\";\nimport UserProfile from \"./profile/UserProfile\";\nimport PostList from \"../posts/PostList\";\nimport NewPost from \"../posts/NewPost\";\nimpor