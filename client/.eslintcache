contexts/AlertContext.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/contexts/AuthContext.js",["130"],"import React, { createContext, useReducer, useEffect } from \"react\";\nimport authReducer from \"../AuthReducer\";\nimport { connectSocket } from \"../socket/Socket\";\n\nconst AuthContext = createContext();\n\nconst AuthProvider = ({ children }) => {\n  const initialState = {\n    isAuthenticated: false,\n    token: null,\n    userID: null,\n    userName: null,\n  };\n  const [state, dispatch] = useReducer(authReducer, initialState);\n\n  useEffect(() => {\n    const token = localStorage.getItem(\"token\");\n    const userID = localStorage.getItem(\"userID\");\n    const userName = localStorage.getItem(\"userName\");\n\n    token &&\n      dispatch({\n        type: \"login\",\n        payload: {\n          token,\n          userID,\n          userName,\n        },\n      });\n  }, []);\n\n  useEffect(() => {}, []);\n\n  return (\n    <AuthContext.Provider value={{ state, dispatch }}>\n      {children}\n    </AuthContext.Provider>\n  );\n};\n\nexport { AuthProvider, AuthContext };\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/Alerts.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/UserSearch.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/layout/MainContent.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/layout/Header.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/profile/UserPage.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/NewPost.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/PostList.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/profile/UserProfile.js",["131"],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/Post.js",["132"],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/comments/CommentSection.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/comments/CommentForm.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/comments/Comment.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/NewsFeed.js",["133"],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/layout/UserPanel.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/FriendRequests.js",["134"],"import React, { useContext, useEffect, useState } from \"react\";\nimport axios from \"axios\";\nimport { AuthContext } from \"../contexts/AuthContext\";\nimport FriendRequestListItem from \"./FriendRequestListItem\";\nimport { disconnectFromSocket, listenForRequests } from \"../socket/Socket\";\nimport { DropDown, Notification } from \"./Utils\";\n\nconst FriendRequests = () => {\n  const [requests, setRequests] = useState([]);\n  const [showDropDown, setShowDropdown] = useState(false);\n  const { state } = useContext(AuthContext);\n\n  useEffect(() => {\n    fetchRequests();\n  }, [state.user]);\n\n  useEffect(() => {\n    listenForRequests(setRequests);\n\n    return () => disconnectFromSocket();\n  }, []);\n\n  const fetchRequests = async () => {\n    try {\n      const config = {\n        headers: {\n          Authorization: `bearer ${state.token}`,\n        },\n      };\n\n      const res = await axios.get(`/api/requests`, config);\n\n      setRequests(res.data);\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  const toggleDropDown = () => {\n    !showDropDown && fetchRequests();\n    setShowDropdown(!showDropDown);\n  };\n\n  return (\n    <div\n      style={{ position: \"relative\", display: \"flex\", alignItems: \"center\" }}\n    >\n      <div>\n        <i\n          style={{ fontSize: \"1.2em\", cursor: \"pointer\" }}\n          className=\"fas fa-user-friends\"\n          onClick={toggleDropDown}\n        />\n        {requests.length > 0 && <Notification>{requests.length}</Notification>}\n        <DropDown show={showDropDown}>\n          <span>Friend requests</span>\n          <ul>\n            {requests.map((request) => (\n              <FriendRequestListItem\n                key={request._id}\n                user={request.user}\n                requests={requests}\n                setRequests={setRequests}\n                requestID={request._id}\n              />\n            ))}\n          </ul>\n          {!requests.length && (\n            <div\n              style={{\n                color: \"#9d9d9d\",\n                marginTop: \"3em\",\n              }}\n            >\n              You have no pending friend requests\n            </div>\n          )}\n        </DropDown>\n      </div>\n    </div>\n  );\n};\n\nexport default FriendRequests;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/FriendRequestListItem.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/SearchResults.js",["135"],"import React, { useEffect, useState } from \"react\";\nimport axios from \"axios\";\nimport { Link, useLocation } from \"react-router-dom\";\n\nconst useQuery = () => {\n  return new URLSearchParams(useLocation().search);\n};\n\nconst SearchResults = () => {\n  const query = useQuery();\n  const [results, setResults] = useState([]);\n\n  useEffect(() => {\n    const fetchResults = async () => {\n      try {\n        const res = await axios.get(`/api/search?user=${query.get(\"user\")}`);\n\n        setResults(res.data);\n      } catch (err) {\n        console.error(err);\n      }\n    };\n    fetchResults();\n  }, []);\n\n  return (\n    <div>\n      {results.map((user) => (\n        <p key={user._id}>\n          <Link to={`/user/${user._id}`}>{user.fullName}</Link>\n        </p>\n      ))}\n    </div>\n  );\n};\n\nexport default SearchResults;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/socket/Socket.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/Notifications.js",["136"],"import React, { useContext, useEffect, useState } from \"react\";\nimport { disconnectFromSocket, listenForNotifications } from \"../socket/Socket\";\nimport axios from \"axios\";\nimport { AuthContext } from \"../contexts/AuthContext\";\nimport { Notification, DropDown } from \"./Utils\";\nimport NotificationListItem from \"./NotificationListItem\";\n\nconst Notifications = () => {\n  const [notifications, setNotifications] = useState([]);\n  const [showDropDown, setShowDropdown] = useState(false);\n  const [notificationCount, setNotificationCount] = useState(0);\n\n  const { state } = useContext(AuthContext);\n\n  useEffect(() => {\n    listenForNotifications(setNotifications);\n\n    return () => disconnectFromSocket();\n  }, []);\n\n  useEffect(() => {\n    setNotificationCount(\n      notifications.reduce(\n        (count, notification) => (notification.seen ? count : count + 1),\n        0\n      )\n    );\n  }, [notifications]);\n\n  useEffect(() => {\n    const fetchNotifications = async () => {\n      try {\n        const config = {\n          headers: {\n            Authorization: `bearer ${state.token}`,\n          },\n        };\n\n        const res = await axios.get(`/api/notifications`, config);\n\n        setNotifications(res.data);\n      } catch (err) {\n        console.error(err);\n      }\n    };\n    fetchNotifications();\n  }, []);\n\n  const markNotificationsAsSeen = async () => {\n    try {\n      const config = {\n        headers: {\n          Authorization: `bearer ${state.token}`,\n        },\n      };\n\n      const res = await axios.put(\"/api/notifications\", {}, config);\n\n      setTimeout(() => setNotificationCount(res.data), 3000);\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  const toggleDropDown = () => {\n    !showDropDown && markNotificationsAsSeen();\n\n    setShowDropdown(!showDropDown);\n  };\n\n  return (\n    <div style={{ position: \"relative\" }}>\n      <i\n        className=\"fas fa-bell\"\n        style={{ fontSize: \"1.2em\", cursor: \"pointer\" }}\n        onClick={toggleDropDown}\n      ></i>\n      {notificationCount > 0 && (\n        <Notification>{notificationCount}</Notification>\n      )}\n      <DropDown show={showDropDown}>\n        <span>Notifications</span>\n        <ul>\n          {notifications.map((notification) => (\n            <NotificationListItem\n              key={notification._id}\n              notification={notification}\n            />\n          ))}\n        </ul>\n        {!notifications.length && (\n          <div\n            style={{\n              color: \"#9d9d9d\",\n              marginTop: \"3em\",\n            }}\n          >\n            You have no pending friend requests\n          </div>\n        )}\n      </DropDown>\n    </div>\n  );\n};\n\nexport default Notifications;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/NotificationListItem.js",["137"],"import React, { useEffect, useReducer } from \"react\";\nimport { Link } from \"react-router-dom\";\nimport { formatDistanceToNow, parseISO } from \"date-fns\";\n\nconst notificationReducer = (state, action) => {\n  switch (action.type) {\n    case \"like\":\n      return { message: ` liked your post` };\n    case \"comment\":\n      return { message: ` commented on your post` };\n    case \"post\":\n      return { message: ` posted on your wall` };\n    default:\n      return state;\n  }\n};\n\nconst NotificationListItem = ({ notification }) => {\n  const { sender, post, type, date, seen } = notification;\n  const initialState = { message: null };\n  const [state, dispatch] = useReducer(notificationReducer, initialState);\n\n  useEffect(() => {\n    dispatch({ type });\n  }, []);\n\n  return (\n    <li>\n      <Link to={`/posts/${post}`}>\n        <div\n          style={{\n            display: \"flex\",\n            flexDirection: \"column\",\n            textAlign: \"left\",\n            padding: \"6px 0\",\n            backgroundColor: seen ? \"#fff\" : \"#c3c3c3\",\n          }}\n        >\n          {sender.fullName + state.message}\n          <time>\n            {formatDistanceToNow(parseISO(date), {\n              includeSeconds: true,\n              addSuffix: true,\n            })}\n          </time>\n        </div>\n      </Link>\n    </li>\n  );\n};\n\nexport default NotificationListItem;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/profile/FriendCard.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/ScrollToTop.js",[],{"ruleId":"138","replacedBy":"139"},{"ruleId":"140","replacedBy":"141"},{"ruleId":"142","severity":1,"message":"143","line":3,"column":10,"nodeType":"144","messageId":"145","endLine":3,"endColumn":23},{"ruleId":"142","severity":1,"message":"146","line":2,"column":17,"nodeType":"144","messageId":"145","endLine":2,"endColumn":25},{"ruleId":"147","severity":1,"message":"148","line":74,"column":6,"nodeType":"149","endLine":74,"endColumn":8,"suggestions":"150"},{"ruleId":"147","severity":1,"message":"151","line":27,"column":6,"nodeType":"149","endLine":27,"endColumn":8,"suggestions":"152"},{"ruleId":"147","severity":1,"message":"153","line":15,"column":6,"nodeType":"149","endLine":15,"endColumn":18,"suggestions":"154"},{"ruleId":"147","severity":1,"message":"155","line":24,"column":6,"nodeType":"149","endLine":24,"endColumn":8,"suggestions":"156"},{"ruleId":"147","severity":1,"message":"151","line":47,"column":6,"nodeType":"149","endLine":47,"endColumn":8,"suggestions":"157"},{"ruleId":"147","severity":1,"message":"158","line":25,"column":6,"nodeType":"149","endL