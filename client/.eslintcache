ent/src/AuthReducer.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/auth/LogIn.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/auth/CreateAccount.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/contexts/AlertContext.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/contexts/AuthContext.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/layout/MainContent.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/layout/Header.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/NewPost.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/PostList.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/NewsFeed.js",["188"],"import axios from \"axios\";\nimport React, { useContext, useEffect, useState } from \"react\";\nimport { AuthContext } from \"../../contexts/AuthContext\";\nimport { LoadingContext } from \"../../contexts/LoadingContext\";\nimport NewPost from \"./NewPost\";\nimport PostList from \"./PostList\";\n\nconst NewsFeed = () => {\n  const { state } = useContext(AuthContext);\n  const { setLoading } = useContext(LoadingContext);\n\n  useEffect(() => {\n    document.title = \"Odinbook - Newsfeed\";\n  }, []);\n\n  const [posts, setPosts] = useState([]);\n  useEffect(() => {\n    const fetchPosts = async () => {\n      setLoading(true);\n      try {\n        const config = {\n          headers: {\n            Authorization: `bearer ${state.token}`,\n          },\n        };\n        const res = await axios.get(\"/api/posts/feed\", config);\n\n        setPosts(res.data);\n      } catch (err) {\n        console.error(err);\n      }\n      setLoading(false);\n    };\n    fetchPosts();\n  }, []);\n\n  return (\n    <div style={{ maxWidth: \"800px\", margin: \"auto\" }}>\n      <NewPost setPosts={setPosts} />\n      <PostList posts={posts} />\n    </div>\n  );\n};\n\nexport default NewsFeed;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/layout/UserPanel.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/socket/Socket.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/contexts/LoadingContext.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/search/SearchResults.js",["189"],"import React, { useEffect, useState } from \"react\";\nimport axios from \"axios\";\nimport { Link, useLocation } from \"react-router-dom\";\nimport UserCard from \"../users/UserCard\";\n\nconst useQuery = () => {\n  return new URLSearchParams(useLocation().search);\n};\n\nconst SearchResults = () => {\n  const query = useQuery();\n  const [results, setResults] = useState([]);\n\n  useEffect(() => {\n    document.title = `Search results - ${query.get(\"user\")}`;\n  }, [query]);\n\n  useEffect(() => {\n    const fetchResults = async () => {\n      try {\n        const res = await axios.get(`/api/search?user=${query.get(\"user\")}`);\n\n        setResults(res.data);\n      } catch (err) {\n        console.error(err);\n      }\n    };\n    fetchResults();\n  }, [query]);\n\n  return results.map((user) => <UserCard user={user} />);\n};\n\nexport default SearchResults;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/search/UserSearch.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/notifications/Notifications.js",["190"],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/notifications/NotificationListItem.js",["191","192"],"import React, { useEffect, useReducer, useState } from \"react\";\nimport { Link } from \"react-router-dom\";\nimport { formatDistanceToNow, parseISO } from \"date-fns\";\n\nconst notificationReducer = (state, action) => {\n  switch (action.type) {\n    case \"like\":\n      return { message: ` liked your post` };\n    case \"comment\":\n      return { message: ` commented on your post` };\n    case \"post\":\n      return { message: ` posted on your wall` };\n    default:\n      return state;\n  }\n};\n\nconst NotificationListItem = ({ notification }) => {\n  const { sender, post, type, date } = notification;\n  const initialState = { message: null };\n  const [state, dispatch] = useReducer(notificationReducer, initialState);\n  const [seen, setSeen] = useState(notification.seen);\n\n  useEffect(() => {\n    dispatch({ type });\n  }, []);\n\n  return (\n    <li onClick={() => setSeen(true)}>\n      <Link to={`/posts/${post}`}>\n        <div\n          style={{\n            display: \"flex\",\n            alignItems: \"center\",\n            borderBottom: \"1px solid #d9d9d9\",\n            borderRadius: \"6px\",\n            padding: \".4rem\",\n            gap: \".4rem\",\n            margin: \".2rem 0\",\n            backgroundColor: seen ? \"#fff\" : \"#e6efff\",\n          }}\n        >\n          <img className=\"small round thumbnail\" src={sender.profilePic} />\n          <div\n            style={{\n              display: \"flex\",\n              flexDirection: \"column\",\n              textAlign: \"left\",\n            }}\n          >\n            <span>\n              <span className=\"bold\">{sender.fullName}</span> {state.message}\n            </span>\n            <time style={{ color: \"#7a7a7a\" }}>\n              {formatDistanceToNow(parseISO(date), {\n                includeSeconds: true,\n                addSuffix: true,\n              })}\n            </time>\n          </div>\n        </div>\n      </Link>\n    </li>\n  );\n};\n\nexport default NotificationListItem;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/notifications/FriendRequests.js",["193"],"import React, { useContext, useEffect, useState } from \"react\";\nimport axios from \"axios\";\nimport { AuthContext } from \"../../contexts/AuthContext\";\nimport FriendRequestListItem from \"./FriendRequestListItem\";\nimport { disconnectFromSocket, listenForRequests } from \"../../socket/Socket\";\nimport { DropDown, Notification } from \"../utils/Utils\";\n\nconst FriendRequests = () => {\n  const [requests, setRequests] = useState([]);\n  const [showDropDown, setShowDropdown] = useState(false);\n  const { state } = useContext(AuthContext);\n\n  useEffect(() => {\n    fetchRequests();\n  }, [state.user]);\n\n  useEffect(() => {\n    listenForRequests(setRequests);\n\n    return () => disconnectFromSocket();\n  }, []);\n\n  const fetchRequests = async () => {\n    try {\n      const config = {\n        headers: {\n          Authorization: `bearer ${state.token}`,\n        },\n      };\n\n      const res = await axios.get(`/api/requests`, config);\n\n      setRequests(res.data);\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  const toggleDropDown = () => {\n    !showDropDown && fetchRequests();\n    setShowDropdown(!showDropDown);\n  };\n\n  return (\n    <div\n      style={{ position: \"relative\", display: \"flex\", alignItems: \"center\" }}\n    >\n      <div>\n        <i\n          style={{ fontSize: \"1.2em\", cursor: \"pointer\" }}\n          className=\"fas fa-user-friends\"\n          onClick={toggleDropDown}\n        />\n        {requests.length > 0 && <Notification>{requests.length}</Notification>}\n        <DropDown show={showDropDown}>\n          <span>Friend requests</span>\n          <ul>\n            {requests.map((request) => (\n              <FriendRequestListItem\n                key={request._id}\n                user={request.user}\n                requests={requests}\n                setRequests={setRequests}\n                requestID={request._id}\n              />\n            ))}\n          </ul>\n          {!requests.length && (\n            <div\n              style={{\n                color: \"#9d9d9d\",\n                marginTop: \"3em\",\n              }}\n            >\n              You have no pending friend requests\n            </div>\n          )}\n        </DropDown>\n      </div>\n    </div>\n  );\n};\n\nexport default FriendRequests;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/notifications/FriendRequestListItem.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/utils/Utils.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/utils/Alerts.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/utils/LoadingSpinner.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/utils/ScrollToTop.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/layout/MobileMenu.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/layout/MobilePanel.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/PostPage.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/hooks/useFriendshipStatus.js",["194"],"import { useEffect, useState } from \"react\";\n\nconst useFriendShipStatus = (currentUserID, user) => {\n  const [friendshipStatus, setFriendshipStatus] = useState(null);\n\n  useEffect(() => {\n    if (isCurrentUserProfile) {\n      setFriendshipStatus(\"isUser\");\n    } else if (userIsFriend) {\n      setFriendshipStatus(\"isFriend\");\n    } else if (requestIsPending) {\n      setFriendshipStatus(\"isPending\");\n    } else {\n      setFriendshipStatus(null);\n    }\n  }, [user]);\n\n  const isCurrentUserProfile = currentUserID === user._id;\n\n  const userIsFriend = user.friends.some(\n    (friend) =>\n      friend.user._id === currentUserID && friend.status === \"accepted\"\n  );\n\n  const requestIsPending = user.friends.some(\n    (friend) =>\n      friend.user._id === currentUserID && friend.status === \"recieved\"\n  );\n\n  return [friendshipStatus, setFriendshipStatus];\n};\n\nexport default useFriendShipStatus;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/UserCard.js",["195"],"import React from \"react\";\nimport { Link } from \"react-router-dom\";\n\nconst UserCard = ({ user }) => {\n  return (\n    <Link to={`/user/${user._id}`}>\n      <div\n        className=\"card\"\n        style={{ display: \"flex\", gap: \".5rem\", margin: \"8px 0\" }}\n      >\n        <img className=\"round thumbnail\" src={user.profilePic} />\n        <div\n          style={{\n            display: \"flex\",\n            flexDirection: \"column\",\n            justifyContent: \"center\",\n          }}\n        >\n          <h2 className=\"bold\">{user.fullName}</h2>\n          <span>{user.profile.location}</span>\n        </div>\n      </div>\n    </Link>\n  );\n};\n\nexport default UserCard;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/Friends.js",["196"],"import React, { useContext, useEffect, useState } from \"react\";\nimport axios from \"axios\";\nimport { useParams } from \"react-router-dom\";\nimport { AuthContext } from \"../../contexts/AuthContext\";\nimport UserCard from \"./UserCard\";\n\nconst Friends = () => {\n  const { state } = useContext(AuthContext);\n  const { userID } = useParams();\n  const [user, setUser] = useState(null);\n\n  useEffect(() => {\n    fetchUser();\n  }, []);\n\n  useEffect(() => {\n    if (user) {\n      document.title = `Odinbook - ${user.firstName}'s friends`;\n    }\n  });\n\n  const fetchUser = async () => {\n    const config = {\n      headers: {\n        Authorization: `bearer ${state.token}`,\n      },\n    };\n\n    const res = await axios.get(`/api/users/${userID}`, config);\n\n    setUser(res.data);\n  };\n\n  return (\n    <div>\n      {user && <h2>{user.fullName}'s friends</h2>}\n      {user && user.friends.map((friend) => <UserCard user={friend.user} />)}\n    </div>\n  );\n};\n\nexport default Friends;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/profile/UserPage.js",["197","198"],"import React, { useContext, useEffect, useState } from \"react\";\nimport axios from \"axios\";\nimport { useParams } from \"react-router-dom\";\nimport UserProfile from \"./UserProfile\";\nimport PostList from \"../../posts/PostList\";\nimport NewPost from \"../../posts/NewPost\";\nimport { LoadingContext } from \"../../../contexts/LoadingContext\";\nimport styled from \"styled-components\";\n\nconst UserPageWrapper = styled.div`\n  display: grid;\n  grid-template-columns: 1fr 2fr;\n  gap: 24px;\n\n  @media screen and (max-width: 800px) {\n    grid-template-columns: 1fr;\n  }\n`;\n\nconst UserPage = () => {\n  const { userID } = useParams();\n  const { setLoading } = useContext(LoadingContext);\n\n  const [user, setUser] = useState(null);\n  useEffect(() => {\n    const fetchUser = async () => {\n      setLoading(true);\n      try {\n        const res = await axios.get(`/api/users/${userID}`);\n\n        document.title = `Odinbook - ${res.data.fullName}`;\n        setUser(res.data);\n      } catch (err) {\n        console.error(err);\n      }\n    };\n    fetchUser();\n  }, [userID]);\n\n  const [posts, setPosts] = useState([]);\n  useEffect(() => {\n    const fetchPosts = async () => {\n      try {\n        const res = await axios.get(`/api/posts/user/${userID}/wall`);\n\n        setPosts(res.data);\n      } catch (err) {\n        console.error(err);\n      }\n      setLoading(false);\n    };\n    fetchPosts();\n  }, [userID]);\n\n  return (\n    <UserPageWrapper>\n      {user && <UserProfile user={user} />}\n      <div>\n        <NewPost setPosts={setPosts} userID={userID} />\n        <PostList posts={posts} />\n      </div>\n    </UserPageWrapper>\n  );\n};\n\nexport default UserPage;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/profile/EditProfile.js",["199","200"],"import React, { useContext, useState, useEffect } from \"react\";\nimport axios from \"axios\";\nimport { AuthContext } from \"../../../contexts/AuthContext\";\nimport { Button, TextInput } from \"../../utils/Utils\";\nimport { LoadingContext } from \"../../../contexts/LoadingContext\";\nimport { useHistory } from \"react-router-dom\";\nimport { AlertContext } from \"../../../contexts/AlertContext\";\n\nconst EditProfile = () => {\n  const history = useHistory();\n  const { token } = useContext(AuthContext).state;\n  const { setAlerts } = useContext(AlertContext);\n  const { setLoading } = useContext(LoadingContext);\n  const [image, setImage] = useState(\"\");\n  const [profileData, setProfileData] = useState({\n    location: \"\",\n    occupation: \"\",\n    bio: \"\",\n  });\n\n  useEffect(() => {\n    document.title = \"Edit Profile\";\n  }, []);\n\n  useEffect(() => {\n    const fetchProfile = async () => {\n      setLoading(true);\n      try {\n        const config = {\n          headers: {\n            \"Content-Type\": \"application/json\",\n            Authorization: `bearer ${token}`,\n          },\n        };\n        const res = await axios.get(`/api/users/profile`, config);\n        const profile = res.data;\n\n        setProfileData(profile);\n      } catch (err) {\n        console.error(err);\n      }\n      setLoading(false);\n    };\n    fetchProfile();\n  }, []);\n\n  const handleFile = (e) => {\n    const file = e.target.files[0];\n    const reader = new FileReader();\n    reader.readAsDataURL(file);\n    reader.onloadend = () => {\n      setImage(reader.result);\n    };\n  };\n\n  const handleChange = (e) => {\n    setProfileData({\n      ...profileData,\n      [e.target.name]: e.target.value,\n    });\n  };\n\n  const handleSubmit = async (e) => {\n    e.preventDefault();\n    setLoading(true);\n    try {\n      const config = {\n        headers: {\n          \"Content-Type\": \"application/json\",\n          Authorization: `bearer ${token}`,\n        },\n      };\n      const { location, occupation, bio } = profileData;\n      const body = JSON.stringify({ location, occupation, bio, image });\n\n      const res = await axios.put(`/api/users/profile`, body, config);\n      const userID = res.data;\n\n      history.push(`/user/${userID}`);\n    } catch (err) {\n      const errorArray = err.response.data.errors.map((err) => {\n        return { text: err.msg, type: \"danger\" };\n      });\n      setAlerts(errorArray);\n    }\n    setLoading(false);\n    setTimeout(() => setAlerts([]), 5000);\n  };\n\n  return (\n    <div>\n      <form className=\"user-form\" onSubmit={(e) => handleSubmit(e)}>\n        <header className=\"form-header\">\n          <h2>Edit your profile</h2>\n        </header>\n        <div className=\"form-group\">\n          <label htmlFor=\"location\">Location:</label>\n          <TextInput\n            id=\"location\"\n            name=\"location\"\n            value={profileData.location}\n            onChange={(e) => handleChange(e)}\n          />\n        </div>\n        <div className=\"form-group\">\n          <label htmlFor=\"occupation\">Occupation:</label>\n          <TextInput\n            id=\"occupation\"\n            name=\"occupation\"\n            value={profileData.occupation}\n            onChange={(e) => handleChange(e)}\n          />\n        </div>\n        <div className=\"form-group\">\n          <label htmlFor=\"bio\">A little about yourself:</label>\n          <TextInput\n            id=\"bio\"\n            name=\"bio\"\n            value={profileData.bio}\n            onChange={(e) => handleChange(e)}\n          />\n        </div>\n        <div className=\"form-group\">\n          <label htmlFor=\"file\">Profile pic:</label>\n          <input\n            type=\"file\"\n            id=\"file\"\n            name=\"file\"\n            onChange={(e) => handleFile(e)}\n          />\n        </div>\n        {image && <img src={image} className=\"large thumbnail\" />}\n        <Button>Save changes</Button>\n      </form>\n    </div>\n  );\n};\n\nexport default EditProfile;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/profile/UserProfile.js",["201"],"import React, { useEffect, useState } from \"react\";\nimport { useMediaQuery } from \"react-responsive\";\nimport styled from \"styled-components\";\nimport { sampleSize } from \"lodash\";\nimport { Link } from \"react-router-dom\";\nimport AddFriendBtn from \"./AddFriendBtn\";\nimport FriendCard from \"./FriendCard\";\n\nconst ProfileSection = styled.section`\n  padding: 0.8rem;\n  background: #fff;\n  border-radius: 8px;\n  box-shadow: 0px 1px 2px #9d9d9d;\n\n  & a:hover {\n    text-decoration: underline;\n  }\n`;\n\nconst ProfileHeader = styled.header`\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n`;\n\nconst ProfilePic = styled.div`\n  margin: auto auto 16px auto;\n  height: auto;\n  background-image: url(\"${({ src }) => src}\");\n  background-size: 100%;\n  background-repeat: no-repeat;\n  background-position: center;\n  width: 70%;\n  padding: 8px;\n  box-shadow: 2px 2px 8px #7d7d7d;\n\n  &:after {\n    content: \"\";\n    display: block;\n    padding-bottom: 100%;\n  }\n`;\n\nconst Profile = ({ user }) => {\n  const isMobile = useMediaQuery({ query: \"(max-width: 800px)\" });\n  const { location, bio, occupation } = user.profile;\n  const [friends, setFriends] = useState([]);\n\n  useEffect(() => {\n    if (!isMobile) {\n      const profileHeight = document.getElementById(\"profile\").scrollHeight;\n      const vh = window.innerHeight;\n      const stickyTop = vh - profileHeight;\n\n      document.getElementById(\"profile\").style.top = `${stickyTop}px`;\n    }\n  });\n\n  useEffect(() => {\n    const filteredFriends = user.friends.filter(\n      (friend) => friend.status === \"accepted\"\n    );\n    setFriends(sampleSize(filteredFriends, isMobile ? 6 : 9));\n  }, [user]);\n\n  return (\n    <div>\n      <div\n        id=\"profile\"\n        style={{\n          marginTop: \"16px\",\n          display: \"flex\",\n          flexDirection: \"column\",\n          gap: \"12px\",\n          position: \"sticky\",\n        }}\n      >\n        <ProfileSection>\n          <ProfilePic src={user.profilePic} />\n          <ProfileHeader>\n            <div>\n              <h2 className=\"bold\">{user.fullName}</h2>\n              <p>{location}</p>\n            </div>\n            <AddFriendBtn user={user} />\n          </ProfileHeader>\n          <div style={{ marginTop: \".8rem\" }}>\n            <div>\n              <h4>Occupation</h4>\n              <p>{occupation}</p>\n            </div>\n            <div style={{ marginTop: \".8rem\" }}>\n              <h4>Bio</h4>\n              <p>{bio}</p>\n            </div>\n          </div>\n        </ProfileSection>\n        <ProfileSection>\n          <ProfileHeader>\n            <h2 className=\"bold\">Friends</h2>\n            <span>\n              <Link to={`/users/${user._id}/friends`}>View all</Link>\n            </span>\n          </ProfileHeader>\n          <div\n            style={{\n              display: \"flex\",\n              justifyContent: \"space-between\",\n              flexWrap: \"wrap\",\n              marginTop: \"10px\",\n              gap: \"8px\",\n            }}\n          >\n            {friends.map((friend) => (\n              <FriendCard key={friend._id} friend={friend} />\n            ))}\n          </div>\n        </ProfileSection>\n        'Odinbook', created by Paul Firmin, 2021\n      </div>\n    </div>\n  );\n};\n\nexport default Profile;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/profile/FriendCard.js",["202"],"import React from \"react\";\nimport { Link } from \"react-route