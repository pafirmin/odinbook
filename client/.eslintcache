seState(notification.seen);\n\n  useEffect(() => {\n    dispatch({ type });\n  }, []);\n\n  return (\n    <li onClick={() => setSeen(true)}>\n      <Link to={`/posts/${post}`}>\n        <NotificationContainer seen={seen}>\n          <img className=\"small round thumbnail\" src={sender.profilePic} />\n          <div\n            style={{\n              display: \"flex\",\n              flexDirection: \"column\",\n              textAlign: \"left\",\n            }}\n          >\n            <span>\n              <span className=\"bold\">{sender.fullName}</span> {state.message}\n            </span>\n            <time style={{ color: \"#7a7a7a\" }}>\n              {formatDistanceToNow(parseISO(date), {\n                includeSeconds: true,\n                addSuffix: true,\n              })}\n            </time>\n          </div>\n        </NotificationContainer>\n      </Link>\n    </li>\n  );\n};\n\nexport default NotificationListItem;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/notifications/FriendRequests.js",["254"],"import React, { useContext, useEffect, useState } from \"react\";\nimport axios from \"axios\";\nimport { AuthContext } from \"../../contexts/AuthContext\";\nimport FriendRequestListItem from \"./FriendRequestListItem\";\nimport { DropDown, Notification } from \"../utils/Utils\";\n\nconst FriendRequests = ({ activeDropdown, toggleDropdown }) => {\n  const [requests, setRequests] = useState([]);\n  const { authState } = useContext(AuthContext);\n\n  const handleRecieveRequest = (request) => setRequests([request, ...requests]);\n\n  useEffect(() => {\n    fetchRequests();\n  }, []);\n\n  useEffect(() => {\n    activeDropdown === 3 && fetchRequests();\n  }, [authState.user, activeDropdown]);\n\n  useEffect(() => {\n    const { socket } = authState;\n    socket.on(\"recieveRequest\", handleRecieveRequest);\n\n    return () => socket.off(\"recieveRequest\", handleRecieveRequest);\n  }, []);\n\n  const fetchRequests = async () => {\n    try {\n      const res = await axios.get(`/api/requests`);\n\n      setRequests(res.data);\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  return (\n    <div\n      style={{ position: \"relative\", display: \"flex\", alignItems: \"center\" }}\n    >\n      <div>\n        <i\n          style={{ fontSize: \"1.2em\", cursor: \"pointer\" }}\n          className=\"fas fa-user-friends\"\n          onClick={toggleDropdown}\n        />\n        {requests.length > 0 && <Notification>{requests.length}</Notification>}\n        {activeDropdown === 3 && (\n          <DropDown>\n            <span>Friend requests</span>\n            <ul>\n              {requests.map((request) => (\n                <FriendRequestListItem\n                  key={request._id}\n                  user={request.user}\n                  requests={requests}\n                  setRequests={setRequests}\n                  requestID={request._id}\n                />\n              ))}\n            </ul>\n            {!requests.length && (\n              <div\n                style={{\n                  color: \"#9d9d9d\",\n                  marginTop: \"3em\",\n                }}\n              >\n                You have no pending friend requests\n              </div>\n            )}\n          </DropDown>\n        )}\n      </div>\n    </div>\n  );\n};\n\nexport default FriendRequests;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/notifications/FriendRequestListItem.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/utils/Utils.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/utils/Alerts.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/utils/LoadingSpinner.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/utils/ScrollToTop.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/PostPage.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/hooks/useFriendshipStatus.js",["255"],"import { useEffect, useState } from \"react\";\n\nconst useFriendShipStatus = (currentUserID, user) => {\n  const [friendshipStatus, setFriendshipStatus] = useState(null);\n\n  useEffect(() => {\n    if (isCurrentUserProfile) {\n      setFriendshipStatus(\"isUser\");\n    } else if (userIsFriend) {\n      setFriendshipStatus(\"isFriend\");\n    } else if (requestIsPending) {\n      setFriendshipStatus(\"isPending\");\n    } else {\n      setFriendshipStatus(null);\n    }\n  }, [user]);\n\n  const isCurrentUserProfile = currentUserID === user._id;\n\n  const userIsFriend = user.friends.some(\n    (friend) =>\n      friend.user._id === currentUserID && friend.status === \"accepted\"\n  );\n\n  const requestIsPending = user.friends.some(\n    (friend) =>\n      friend.user._id === currentUserID && friend.status === \"recieved\"\n  );\n\n  return { friendshipStatus, setFriendshipStatus };\n};\n\nexport default useFriendShipStatus;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/UserCard.js",["256"],"import React from \"react\";\nimport { Link } from \"react-router-dom\";\n\nconst UserCard = ({ user }) => {\n  return (\n    <Link to={`/user/${user._id}`}>\n      <div\n        className=\"card\"\n        style={{ display: \"flex\", gap: \".5rem\", margin: \"8px 0\" }}\n      >\n        <img className=\"round thumbnail\" src={user.profilePic} />\n        <div\n          style={{\n            display: \"flex\",\n            flexDirection: \"column\",\n            justifyContent: \"center\",\n          }}\n        >\n          <h2 className=\"bold\">{user.fullName}</h2>\n          <span>{user.profile.location}</span>\n        </div>\n      </div>\n    </Link>\n  );\n};\n\nexport default UserCard;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/Friends.js",["257"],"import React, { useEffect, useState } from \"react\";\nimport axios from \"axios\";\nimport { useParams } from \"react-router-dom\";\nimport UserCard from \"./UserCard\";\n\nconst Friends = () => {\n  const { userID } = useParams();\n  const [user, setUser] = useState(null);\n\n  useEffect(() => {\n    fetchUser();\n  }, []);\n\n  useEffect(() => {\n    if (user) {\n      document.title = `Odinbook - ${user.firstName}'s friends`;\n    }\n  }, [user]);\n\n  const fetchUser = async () => {\n    try {\n      const res = await axios.get(`/api/users/${userID}`);\n\n      setUser(res.data);\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  return (\n    <div>\n      {user && <h2>{user.fullName}'s friends</h2>}\n      {user &&\n        user.friends.map((friend) => (\n          <UserCard key={friend._id} user={friend.user} />\n        ))}\n    </div>\n  );\n};\n\nexport default Friends;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/profile/EditProfile.js",["258","259"],"import React, { useContext, useState, useEffect } from \"react\";\nimport axios from \"axios\";\nimport { Button, TextInput } from \"../../utils/Utils\";\nimport { LoadingContext } from \"../../../contexts/LoadingContext\";\nimport { useHistory } from \"react-router-dom\";\nimport { AlertContext } from \"../../../contexts/AlertContext\";\n\nconst EditProfile = () => {\n  const history = useHistory();\n  const { setAlerts } = useContext(AlertContext);\n  const { setLoading } = useContext(LoadingContext);\n  const [image, setImage] = useState(\"\");\n  const [profileData, setProfileData] = useState({\n    location: \"\",\n    occupation: \"\",\n    bio: \"\",\n  });\n\n  useEffect(() => {\n    document.title = \"Edit Profile\";\n  }, []);\n\n  useEffect(() => {\n    const fetchProfile = async () => {\n      setLoading(true);\n      try {\n        const res = await axios.get(`/api/users/profile`);\n        const profile = res.data;\n\n        setProfileData(profile);\n      } catch (err) {\n        console.error(err);\n      }\n      setLoading(false);\n    };\n    fetchProfile();\n  }, []);\n\n  const handleFile = (e) => {\n    const file = e.target.files[0];\n    const reader = new FileReader();\n    reader.readAsDataURL(file);\n    reader.onloadend = () => {\n      setImage(reader.result);\n    };\n  };\n\n  const handleChange = (e) => {\n    setProfileData({\n      ...profileData,\n      [e.target.name]: e.target.value,\n    });\n  };\n\n  const handleSubmit = async (e) => {\n    e.preventDefault();\n    setLoading(true);\n    try {\n      const { location, occupation, bio } = profileData;\n      const body = JSON.stringify({ location, occupation, bio, image });\n\n      const res = await axios.put(`/api/users/profile`, body);\n      const userID = res.data;\n\n      history.push(`/user/${userID}`);\n    } catch (err) {\n      const errorArray = err.response.data.errors.map((err) => {\n        return { text: err.msg, type: \"danger\" };\n      });\n      setAlerts(errorArray);\n    } finally {\n      setLoading(false);\n      setTimeout(() => setAlerts([]), 5000);\n    }\n  };\n\n  return (\n    <div>\n      <form className=\"user-form\" onSubmit={(e) => handleSubmit(e)}>\n        <header className=\"form-header\">\n          <h2>Edit your profile</h2>\n        </header>\n        <div className=\"form-group\">\n          <label htmlFor=\"location\">Location:</label>\n          <TextInput\n            id=\"location\"\n            name=\"location\"\n            value={profileData.location}\n            onChange={(e) => handleChange(e)}\n          />\n        </div>\n        <div className=\"form-group\">\n          <label htmlFor=\"occupation\">Occupation:</label>\n          <TextInput\n            id=\"occupation\"\n            name=\"occupation\"\n            value={profileData.occupation}\n            onChange={(e) => handleChange(e)}\n          />\n        </div>\n        <div className=\"form-group\">\n          <label htmlFor=\"bio\">A little about yourself:</label>\n          <TextInput\n            id=\"bio\"\n            name=\"bio\"\n            value={profileData.bio}\n            onChange={(e) => handleChange(e)}\n          />\n        </div>\n        <div className=\"form-group\">\n          <label htmlFor=\"file\">Profile pic:</label>\n          <input\n            type=\"file\"\n            id=\"file\"\n            name=\"file\"\n            onChange={(e) => handleFile(e)}\n          />\n        </div>\n        {image && <img src={image} className=\"large thumbnail\" />}\n        <Button>Save changes</Button>\n      </form>\n    </div>\n  );\n};\n\nexport default EditProfile;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/profile/UserProfile.js",["260"],"import React, { useEffect, useContext, useState, useCallback } from \"react\";\nimport styled from \"styled-components\";\nimport { AuthContext } from \"../../../contexts/AuthContext\";\nimport useFriendshipStatus from \"../../../hooks/useFriendshipStatus\";\nimport ProfileTop from \"./ProfileTop\";\nimport ProfileDetails from \"./ProfileDetails\";\nimport ProfileFriends from \"./ProfileFriends\";\nimport { useMediaQuery } from \"react-responsive\";\n\nconst ProfileWrapper = styled.div`\n  margin-top: 16px;\n  display: flex;\n  flex-direction: column;\n  position: sticky;\n  top: ${({ top }) => top}px;\n\n  & > * + * {\n    margin-top: 1rem;\n  }\n`;\n\nconst ProfileSection = styled.section`\n  padding: 0.8rem;\n  background: ${(props) => props.theme.cardBg};\n  border-radius: 8px;\n  box-shadow: 0px 1px 2px ${(props) => props.theme.shadowColour};\n  display: flex;\n  flex-direction: column;\n\n  & > * + * {\n    margin-top: 0.8rem;\n  }\n\n  & a:hover {\n    text-decoration: underline;\n  }\n`;\n\nconst Profile = ({ user }) => {\n  const { authState } = useContext(AuthContext);\n  const { friendshipStatus } = useFriendshipStatus(authState.userID, user);\n  const [friends, setFriends] = useState([]);\n  const [stickyTop, setStickyTop] = useState();\n  const isMobile = useMediaQuery({ query: \"(max-width: 800px)\" });\n\n  const filteredFriends = useCallback(() => {\n    return user.friends.filter((friend) => friend.status === \"accepted\");\n  }, [user]);\n\n  const stickyRef = useCallback(\n    (node) => {\n      if (node) {\n        setStickyTop(window.innerHeight - node.scrollHeight);\n      }\n    },\n    [friends]\n  );\n\n  useEffect(() => {\n    setFriends(filteredFriends());\n  }, [filteredFriends, isMobile]);\n\n  return (\n    <div>\n      <ProfileWrapper top={stickyTop} ref={stickyRef}>\n        <ProfileTop user={user} friendshipStatus={friendshipStatus} />\n        <ProfileSection>\n          <ProfileDetails user={user} friendshipStatus={friendshipStatus} />\n        </ProfileSection>\n        <ProfileSection>\n          <ProfileFriends user={user} friends={friends} isMobile={isMobile} />\n        </ProfileSection>\n        {!isMobile && (\n          <footer style={{ textAlign: \"center\" }}>\n            'Odinbook', created by Paul Firmin, 2021\n          </footer>\n        )}\n      </ProfileWrapper>\n    </div>\n  );\n};\n\nexport default Profile;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/profile/FriendCard.js",["261"],"import React from \"react\";\nimport { Link } from \"react-router-dom\";\nconst FriendCard = ({ friend }) => {\n  return (\n    <div style={{ width: \"30%\", textAlign: \"center\" }}>\n      <Link to={`/user/${friend.user._id}`}>\n        <img className=\"large thumbnail\" src={friend.user.profilePic} />\n        <figcaption style={{ fontSize: \".85rem\", fontWeight: \"900\" }}>\n          {friend.user.fullName}\n        </figcaption>\n      </Link>\n    </div>\n  );\n};\n\nexport default FriendCard;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/profile/AddFriendBtn.js",["262"],"import axios from \"axios\";\nimport React, { useContext } from \"react\";\nimport styled from \"styled-components\";\nimport useFriendshipStatus from \"../../../hooks/useFriendshipStatus\";\nimport { AuthContext } from \"../../../contexts/AuthContext\";\nimport { Button } from \"../../utils/Utils\";\nimport { sendFriendRequest } from \"../../../socket/Socket\";\nimport { Link } from \"react-router-dom\";\n\nconst ProfileBtn = styled(Button)`\n  font-size: 0.8em;\n  padding: 2px 8px;\n`;\n\nconst AddFriendBtn = ({ user }) => {\n  const { authState } = useContext(AuthContext);\n  const { friendshipStatus, setFriendshipStatus } = useFriendshipStatus(\n    authState.userID,\n    user\n  );\n\n  const addFriend = async () => {\n    try {\n      const res = await axios.post(`/api/requests/${user._id}`, {});\n\n      sendFriendRequest(res.data);\n      setFriendshipStatus(\"isPending\");\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  const getButton = () => {\n    switch (friendshipStatus) {\n      case \"isUser\":\n        return null;\n      case \"isFriend\":\n        return (\n          <ProfileBtn variant=\"success\">\n            <i className=\"fas fa-check\" /> Friends\n          </ProfileBtn>\n        );\n      case \"isPending\":\n        return <ProfileBtn>Request sent!</ProfileBtn>;\n      default:\n        return (\n          <ProfileBtn onClick={addFriend}>\n            {\" \"}\n            <i className=\"fas fa-user-friends\" /> Add as friend\n          </ProfileBtn>\n        );\n    }\n  };\n  return getButton();\n};\n\nexport default AddFriendBtn;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/post/Post.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/post/PostHeader.js",["263"],"import React from \"react\";\nimport { format, formatDistanceToNow, parseISO } from \"date-fns\";\nimport { Link } from \"react-router-do