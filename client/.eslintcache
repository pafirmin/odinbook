93"],"import React, { useContext, useState, useEffect } from \"react\";\nimport axios from \"axios\";\nimport { AuthContext } from \"../../../contexts/AuthContext\";\nimport { Button, TextInput } from \"../../utils/Utils\";\nimport { LoadingContext } from \"../../../contexts/LoadingContext\";\nimport { useHistory } from \"react-router-dom\";\nimport { AlertContext } from \"../../../contexts/AlertContext\";\n\nconst EditProfile = () => {\n  const history = useHistory();\n  const { token } = useContext(AuthContext).state;\n  const { setAlerts } = useContext(AlertContext);\n  const { setLoading } = useContext(LoadingContext);\n  const [image, setImage] = useState(\"\");\n  const [profileData, setProfileData] = useState({\n    location: \"\",\n    occupation: \"\",\n    bio: \"\",\n  });\n\n  useEffect(() => {\n    document.title = \"Edit Profile\";\n  }, []);\n\n  useEffect(() => {\n    const fetchProfile = async () => {\n      setLoading(true);\n      try {\n        const config = {\n          headers: {\n            \"Content-Type\": \"application/json\",\n            Authorization: `bearer ${token}`,\n          },\n        };\n        const res = await axios.get(`/api/users/profile`, config);\n        const profile = res.data;\n\n        setProfileData(profile);\n      } catch (err) {\n        console.error(err);\n      }\n      setLoading(false);\n    };\n    fetchProfile();\n  }, []);\n\n  const handleFile = (e) => {\n    const file = e.target.files[0];\n    const reader = new FileReader();\n    reader.readAsDataURL(file);\n    reader.onloadend = () => {\n      setImage(reader.result);\n    };\n  };\n\n  const handleChange = (e) => {\n    setProfileData({\n      ...profileData,\n      [e.target.name]: e.target.value,\n    });\n  };\n\n  const handleSubmit = async (e) => {\n    e.preventDefault();\n    setLoading(true);\n    try {\n      const config = {\n        headers: {\n          \"Content-Type\": \"application/json\",\n          Authorization: `bearer ${token}`,\n        },\n      };\n      const { location, occupation, bio } = profileData;\n      const body = JSON.stringify({ location, occupation, bio, image });\n\n      const res = await axios.put(`/api/users/profile`, body, config);\n      const userID = res.data;\n\n      history.push(`/user/${userID}`);\n    } catch (err) {\n      const errorArray = err.response.data.errors.map((err) => {\n        return { text: err.msg, type: \"danger\" };\n      });\n      setAlerts(errorArray);\n    }\n    setLoading(false);\n    setTimeout(() => setAlerts([]), 5000);\n  };\n\n  return (\n    <div>\n      <form className=\"user-form\" onSubmit={(e) => handleSubmit(e)}>\n        <header className=\"form-header\">\n          <h2>Edit your profile</h2>\n        </header>\n        <div className=\"form-group\">\n          <label htmlFor=\"location\">Location:</label>\n          <TextInput\n            id=\"location\"\n            name=\"location\"\n            value={profileData.location}\n            onChange={(e) => handleChange(e)}\n          />\n        </div>\n        <div className=\"form-group\">\n          <label htmlFor=\"occupation\">Occupation:</label>\n          <TextInput\n            id=\"occupation\"\n            name=\"occupation\"\n            value={profileData.occupation}\n            onChange={(e) => handleChange(e)}\n          />\n        </div>\n        <div className=\"form-group\">\n          <label htmlFor=\"bio\">A little about yourself:</label>\n          <TextInput\n            id=\"bio\"\n            name=\"bio\"\n            value={profileData.bio}\n            onChange={(e) => handleChange(e)}\n          />\n        </div>\n        <div className=\"form-group\">\n          <label htmlFor=\"file\">Profile pic:</label>\n          <input\n            type=\"file\"\n            id=\"file\"\n            name=\"file\"\n            onChange={(e) => handleFile(e)}\n          />\n        </div>\n        {image && <img src={image} className=\"large thumbnail\" />}\n        <Button>Save changes</Button>\n      </form>\n    </div>\n  );\n};\n\nexport default EditProfile;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/profile/UserProfile.js",["194"],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/profile/FriendCard.js",["195"],"import React from \"react\";\nimport { Link } from \"react-router-dom\";\nconst FriendCard = ({ friend }) => {\n  return (\n    <div style={{ width: \"30%\", textAlign: \"center\" }}>\n      <Link to={`/user/${friend.user._id}`}>\n        <img className=\"large thumbnail\" src={friend.user.profilePic} />\n        <figcaption style={{ fontSize: \".85rem\", fontWeight: \"900\" }}>\n          {friend.user.fullName}\n        </figcaption>\n      </Link>\n    </div>\n  );\n};\n\nexport default FriendCard;\n","/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/users/profile/AddFriendBtn.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/post/Post.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/post/PostHeader.js",["196","197"],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/post/comments/CommentSection.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/post/comments/Comment.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/post/comments/CommentForm.js",[],"/home/paul/Documents/the_odin_project/nodeJs/social-network/client/src/components/posts/post/PostInteraction.js",["198"],{"ruleId":"199","replacedBy":"200"},{"ruleId":"201","replacedBy":"202"},{"ruleId":"203","severity":1,"message":"204","line":35,"column":6,"nodeType":"205","endLine":35,"endColumn":8,"suggestions":"206"},{"ruleId":"207","severity":1,"message":"208","line":3,"column":10,"nodeType":"209","messageId":"210"